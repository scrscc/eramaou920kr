ELSEIF ABL:32 == 1
	TIMES B , 1.20
ELSEIF ABL:32 == 2
	TIMES B , 1.30
ELSEIF ABL:32 == 3
	TIMES B , 1.50
ELSEIF ABL:32 == 4
	TIMES B , 1.70
ELSE
	TIMES B , 2.00
ENDIF

;TALENT:舌使いをみる
SIF TALENT:52
	TIMES B , 2.00

;조수のABL:Ｃ感覚をみる
IF ABL:ASSI:0 == 0
	TIMES B , 1.00
ELSEIF ABL:ASSI:0 == 1
	TIMES B , 1.50
ELSEIF ABL:ASSI:0 == 2
	TIMES B , 2.00
ELSEIF ABL:ASSI:0 == 3
	TIMES B , 2.50
ELSEIF ABL:ASSI:0 == 4
	TIMES B , 3.50
ELSE
	TIMES B , 5.00
ENDIF

SIF TALENT:ASSI:121 || TALENT:ASSI:122
	BASE:ASSI:2 += B


;-------------------------------------------------
;ソースの計算
;-------------------------------------------------
LOSEBASE:0 += 80
LOSEBASE:1 += 250

SOURCE:13 = 6000
SOURCE:14 = 600

;上のほうで計算した汚れデータ
SOURCE:8 = Y*40 + 100

;ABL:奉仕精神をみる
IF ABL:16 == 0
	SOURCE:4 = 800
	SOURCE:5 = 600
	TIMES SOURCE:8 , 4.00
ELSEIF ABL:16 == 1
	SOURCE:4 = 1200
	SOURCE:5 = 900
	TIMES SOURCE:8 , 2.50
ELSEIF ABL:16 == 2
	SOURCE:4 = 1400
	SOURCE:5 = 1000
	TIMES SOURCE:8 , 1.50
ELSEIF ABL:16 == 3
	SOURCE:4 = 1600
	SOURCE:5 = 1200
	TIMES SOURCE:8 , 1.00
ELSEIF ABL:16 == 4
	SOURCE:4 = 1800
	SOURCE:5 = 1500
	TIMES SOURCE:8 , 0.50
ELSE
	SOURCE:4 = 2000
	SOURCE:5 = 2200
	TIMES SOURCE:8 , 0.10
ENDIF


;ABL:技巧をみる
IF ABL:12 == 0
	TIMES SOURCE:4 , 0.80
	TIMES SOURCE:5 , 0.80
ELSEIF ABL:12 == 1
	TIMES SOURCE:4 , 1.00
	TIMES SOURCE:5 , 1.00
ELSEIF ABL:12 == 2
	TIMES SOURCE:4 , 1.20
	TIMES SOURCE:5 , 1.20
ELSEIF ABL:12 == 3
	TIMES SOURCE:4 , 1.50
	TIMES SOURCE:5 , 1.50
