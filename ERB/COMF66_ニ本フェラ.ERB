	;口で사정させたフラグ
	TFLAG:0 = 2
;通常の사정
ELSEIF E == 1
	EXP:MASTER:3 += 1
	EXP:20 += 3
	PRINTL 사정
	PRINTL 정액경험＋３
	;精液汚れ
	STAIN:MASTER:2 |= 4

	BASE:MASTER:2 -= EJAC
	SIF BASE:MASTER:2 >= EJAC
		BASE:MASTER:2 = EJAC-1

	;口で사정させたフラグ
