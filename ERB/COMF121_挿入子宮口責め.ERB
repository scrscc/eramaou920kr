@COM121

TFLAG:42 = 0
IF SELECTCOM == 34
	PRINTL 승마 자궁구 고문
	;SAVESTR:22 = 騎乗子宮口責め
ELSE
	PRINTL 삽입 자궁구 고문
	;SAVESTR:22 = 삽입자궁구자극
ENDIF

SELECTCOM = 121
CALL TRAIN_MESSAGE_B

;V経験を伴うコマンドのフラグ
TFLAG:19 = 1
SIF TALENT:85 && ASSIPLAY == 0 && EXP:0 == 0
	TFLAG:20 = 1

;調教者の사정チェック
CALL COM_EJAC_PLAYER_SEX

;-------------------------------------------------
;ソースの計算
;-------------------------------------------------
IF PREVCOM == 34
	LOSEBASE:0 += 60
	LOSEBASE:1 += 150
	SOURCE:12 = 900
ELSE
	LOSEBASE:0 += 50
	LOSEBASE:1 += 100
	SOURCE:12 = 400
ENDIF

SOURCE:0 = 0

;前方向挿入の場合（정상위・대면좌위・기승위）
IF PREVCOM == 20 || PREVCOM == 22 || PREVCOM == 34
	;ABL:Ｃ感覚をみる
	IF ABL:0 == 0
		SOURCE:0 = 20
	ELSEIF ABL:0 == 1
		SOURCE:0 = 100
	ELSEIF ABL:0 == 2
		SOURCE:0 = 400
	ELSEIF ABL:0 == 3
		SOURCE:0 = 900
	ELSEIF ABL:0 == 4
		SOURCE:0 = 1400
	ELSE
		SOURCE:0 = 2100
	ENDIF
ENDIF

;ABL:Ｖ感覚をみる
IF ABL:2 == 0
	SOURCE:1 = 10
	SOURCE:3 = 100
ELSEIF ABL:2 == 1
	SOURCE:1 = 30
	SOURCE:3 = 180
ELSEIF ABL:2 == 2
	SOURCE:1 = 400
	SOURCE:3 = 250
ELSEIF ABL:2 == 3
	SOURCE:1 = 800
	SOURCE:3 = 380
ELSEIF ABL:2 == 4
	SOURCE:1 = 2700
	SOURCE:3 = 500
ELSE
	SOURCE:1 = 3800
	SOURCE:3 = 800
ENDIF

;EXP:Ｖ経験をみる
IF EXP:0 < EXPLV:1
	TIMES SOURCE:1 , 0.20
	SOURCE:6 = 300
ELSEIF EXP:0 < EXPLV:2
	TIMES SOURCE:1 , 0.60
	SOURCE:6 = 100
ELSEIF EXP:0 < EXPLV:3
	TIMES SOURCE:1 , 1.00
	SOURCE:6 = 10
ELSEIF EXP:0 < EXPLV:4
	TIMES SOURCE:1 , 1.20
	SOURCE:6 = 0
ELSEIF EXP:0 < EXPLV:5
	TIMES SOURCE:1 , 1.30
	SOURCE:6 = 0
ELSE
	TIMES SOURCE:1 , 1.80
	SOURCE:6 = 0
ENDIF

;PALAM:潤滑をみる
IF PALAM:3 < PALAMLV:1
	TIMES SOURCE:0 , 0.50
	TIMES SOURCE:6 , 0.10
	SOURCE:6 += 1000
	TIMES SOURCE:6 , 3.00
ELSEIF PALAM:3 < PALAMLV:2
	TIMES SOURCE:0 , 0.60
	TIMES SOURCE:6, 0.40
	SOURCE:6 += 300
	TIMES SOURCE:6, 1.00
ELSEIF PALAM:3 < PALAMLV:3
	TIMES SOURCE:1 , 1.00
	TIMES SOURCE:6 , 0.50
ELSEIF PALAM:3 < PALAMLV:4
	TIMES SOURCE:1 , 1.40
	TIMES SOURCE:6 , 0.20
ELSE
	TIMES SOURCE:1 , 1.80
	TIMES SOURCE:6 , 0.10
ENDIF

;対面座位だと効果が高い
IF PREVCOM == 22
	TIMES SOURCE:1 , 1.30
	TIMES SOURCE:3 , 1.10
;背面座位だと効果が低い
ELSEIF PREVCOM == 23
	TIMES SOURCE:1 , 0.70
	TIMES SOURCE:3 , 0.90
ENDIF

;調教者が[남자]
SIF TALENT:PLAYER:122
	TIMES SOURCE:1 , 2.50

;小柄体形
SIF TALENT:100
	TIMES SOURCE:6 , 2.00

;정조관념
IF TALENT:30
	IF TALENT:0 == 1
		TIMES SOURCE:3 , 0.60
		TIMES SOURCE:14, 5.00
		SOURCE:15 = 2000
	ELSE
		TIMES SOURCE:3 , 0.60
		SOURCE:15 = 1000
	ENDIF
;정조관부실
ELSEIF TALENT:31
	IF TALENT:0 == 1
		TIMES SOURCE:3 , 0.60
		SOURCE:15 = 300
	ENDIF
ELSE
	SIF TALENT:0 == 1
		SOURCE:15 = 3000
ENDIF

;PALAM:欲情をみる
IF PALAM:5 < PALAMLV:1
	TIMES SOURCE:0 , 0.60
	TIMES SOURCE:1 , 0.60
	TIMES SOURCE:3 , 0.30
ELSEIF PALAM:5 < PALAMLV:2
	TIMES SOURCE:0 , 0.80
	TIMES SOURCE:1 , 0.80
	TIMES SOURCE:3 , 0.60
ELSEIF PALAM:5 < PALAMLV:3
	TIMES SOURCE:0 , 1.00
	TIMES SOURCE:1 , 1.00
	TIMES SOURCE:3 , 1.00
ELSEIF PALAM:5 < PALAMLV:4
	TIMES SOURCE:0 , 1.20
	TIMES SOURCE:1 , 1.20
	TIMES SOURCE:3 , 1.50
ELSE
	TIMES SOURCE:0 , 1.50
	TIMES SOURCE:1 , 1.50
	TIMES SOURCE:3 , 1.80
ENDIF

;ABL:従順をみる
IF ABL:10 == 0
	TIMES SOURCE:0 , 0.50
	TIMES SOURCE:1 , 0.50
	TIMES SOURCE:3 , 0.60
	TIMES SOURCE:15 , 2.00
ELSEIF ABL:10 == 1
	TIMES SOURCE:0 , 0.80
	TIMES SOURCE:1 , 0.80
	TIMES SOURCE:3 , 0.80
	TIMES SOURCE:15 , 1.50
ELSEIF ABL:10 == 2
	TIMES SOURCE:0 , 1.00
	TIMES SOURCE:1 , 1.00
	TIMES SOURCE:3 , 1.00
	TIMES SOURCE:15 , 1.00
ELSEIF ABL:10 == 3
	TIMES SOURCE:0 , 1.20
	TIMES SOURCE:1 , 1.30
	TIMES SOURCE:3 , 1.20
	TIMES SOURCE:15 , 0.80
ELSEIF ABL:10 == 4
	TIMES SOURCE:0 , 1.40
	TIMES SOURCE:1 , 1.60
	TIMES SOURCE:3 , 1.40
	TIMES SOURCE:15 , 0.60
ELSE
	TIMES SOURCE:0 , 1.60
	TIMES SOURCE:1 , 2.00
	TIMES SOURCE:3 , 1.60
	TIMES SOURCE:15 , 0.30
ENDIF

IF PREVCOM == 34
	;ABL:奉仕精神をみる
	IF ABL:13 == 0
		SOURCE:4 = 200
		SOURCE:5 = 50
		SOURCE:14 = 300
	ELSEIF ABL:13 == 1
		SOURCE:4 = 250
		SOURCE:5 = 200
		SOURCE:14 = 100
	ELSEIF ABL:13 == 2
		SOURCE:4 = 350
		SOURCE:5 = 550
		SOURCE:14 = 30
	ELSEIF ABL:13 == 3
		SOURCE:4 = 450
		SOURCE:5 = 900
		SOURCE:14 = 0
	ELSEIF ABL:13 == 4
		SOURCE:4 = 600
		SOURCE:5 = 1500
		SOURCE:14 = 0
	ELSE
		SOURCE:4 = 750
		SOURCE:5 = 2200
		SOURCE:14 = 0
	ENDIF
	;ABL:技巧をみる
	IF ABL:12 == 0
		TIMES SOURCE:1 , 0.30
		TIMES SOURCE:3 , 0.30
	ELSEIF ABL:12 == 1
		TIMES SOURCE:1 , 0.60
		TIMES SOURCE:3 , 0.60
	ELSEIF ABL:12 == 2
		TIMES SOURCE:1 , 1.00
		TIMES SOURCE:3 , 1.00
	ELSEIF ABL:12 == 3
		TIMES SOURCE:1 , 1.10
		TIMES SOURCE:3 , 1.10
	ELSEIF ABL:12 == 4
		TIMES SOURCE:1 , 1.30
		TIMES SOURCE:3 , 1.20
	ELSE
		TIMES SOURCE:1 , 1.50
		TIMES SOURCE:3 , 1.40
	ENDIF
ENDIF

;乱れ牡丹からの派生
IF PREVCOM == 612
	;ABL:Ｂ感覚をみる
	IF ABL:1 == 0
		SOURCE:17 = 50
		SOURCE:3 += 50
		SOURCE:4 += 50
	ELSEIF ABL:1 == 1
		SOURCE:17 = 200
		SOURCE:3 += 200
		SOURCE:4 += 200
	ELSEIF ABL:1 == 2
		SOURCE:17 = 500
		SOURCE:3 += 500
		SOURCE:4 += 400
	ELSEIF ABL:1 == 3
		SOURCE:17 = 1000
		SOURCE:3 += 600
		SOURCE:4 += 600
	ELSEIF ABL:1 == 4
		SOURCE:17 = 1600
		SOURCE:3 += 1000
		SOURCE:4 += 1000
	ELSE
		SOURCE:17 = 2100
		SOURCE:3 += 1400
		SOURCE:4 += 1400
	ENDIF

	;ABL:Ｃ感覚をみる
	IF ABL:0 == 0
		SOURCE:0 += 40
		SOURCE:4 += 50
	ELSEIF ABL:0 == 1
		SOURCE:0 += 160
		SOURCE:4 += 200
	ELSEIF ABL:0 == 2
		SOURCE:0 += 700
		SOURCE:4 += 400
	ELSEIF ABL:0 == 3
		SOURCE:0 += 1500
		SOURCE:4 += 600
	ELSEIF ABL:0 == 4
		SOURCE:0 += 2400
		SOURCE:4 += 1000
	ELSE
		SOURCE:0 += 3600
		SOURCE:4 += 1400
	ENDIF

	;プレイヤーのABL:技巧をみる
	IF ABL:PLAYER:12 == 0
		SOURCE:0 += 100
		SOURCE:1 += 100
		SOURCE:17 += 50
		SOURCE:3 += 100
		SOURCE:4 += 50
		SOURCE:10 += 0
		SOURCE:11 += 0
		A = 0
	ELSEIF ABL:PLAYER:12 == 1
		SOURCE:0 += 200
		SOURCE:1 += 200
		SOURCE:17 += 150
		SOURCE:3 += 150
		SOURCE:4 += 100
		SOURCE:10 += 50
		SOURCE:11 += 100
		A = 0
	ELSEIF ABL:PLAYER:12 == 2
		SOURCE:0 += 300
		SOURCE:1 += 300
		SOURCE:17 += 250
		SOURCE:3 += 200
		SOURCE:4 += 200
		SOURCE:10 += 100
		SOURCE:11 += 200
		A = 50
	ELSEIF ABL:PLAYER:12 == 3
		SOURCE:0 += 500
		SOURCE:1 += 400
		SOURCE:17 += 500
		SOURCE:3 += 300
		SOURCE:4 += 300
		SOURCE:10 += 150
		SOURCE:11 += 300
		A = 100
	ELSEIF ABL:PLAYER:12 == 4
		SOURCE:0 += 800
		SOURCE:1 += 500
		SOURCE:17 += 800
		SOURCE:3 += 500
		SOURCE:4 += 400
		SOURCE:10 += 250
		SOURCE:11 += 400
		A = 300
	ELSE
		SOURCE:0 += 1200
		SOURCE:1 += 600
		SOURCE:17 += 1200
		SOURCE:3 += 800
		SOURCE:4 += 600
		SOURCE:10 += 400
		SOURCE:11 += 500
		A = 500
	ENDIF

	;ABL:露出癖をみる
	IF ABL:17 == 0
		SOURCE:7 += 100
		SOURCE:10 += 100
		TIMES A, 0.60
	ELSEIF ABL:17 == 1
		SOURCE:7 += 200
		SOURCE:10 += 200
		TIMES A, 1.00
	ELSEIF ABL:17 == 2
		SOURCE:7 += 400
		SOURCE:10 += 400
		TIMES A, 1.50
	ELSEIF ABL:17 == 3
		SOURCE:7 += 700
		SOURCE:10 += 700
		TIMES A, 2.40
	ELSEIF ABL:17 == 4
		SOURCE:7 += 1200
		SOURCE:10 += 1200
		TIMES A, 3.60
	ELSE
		SOURCE:7 += 2000
		SOURCE:10 += 2000
		TIMES A, 5.80
	ENDIF

	SOURCE:0 += A
	SOURCE:1 += A
	SOURCE:3 += A

	SIF EXP:0 >= EXPLV:3
		SOURCE:1 += A

	;TALENT:恋慕をみる
	SIF TALENT:85
		TIMES SOURCE:3 , 2.00
ENDIF

;セックス後の処理
