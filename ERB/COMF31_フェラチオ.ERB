;eraIM@Sから導入しました(eramaou)

;-------------------------------------------------
;펠라치오
;奉仕系コマンド：調教対象が調教者のペニスを口で刺激
;-------------------------------------------------
@COM31

;前回と今回の調教者が同じ
IF (ASSIPLAY && TFLAG:50) || (ASSIPLAY == 0 && TFLAG:50 == 0)
	;前回の調教がクンニかフェラするかシックスナインだとシックスナインに
	IF PREVCOM == 1 || PREVCOM == 4 || PREVCOM == 69
		CALL COM_ABLE69
		SIF RESULT == 1
			JUMP COM69
	ENDIF
ENDIF

;前回と今回の調教者が同じ
IF (ASSIPLAY && TFLAG:50) || (ASSIPLAY == 0 && TFLAG:50 == 0)
	;前回の調教がパイズリだとパイズリフェラに
	IF PREVCOM == 32
		CALL COM_ABLE123
		SIF RESULT == 1
			JUMP COM123
	ENDIF
ENDIF

;前回と今回の調教者が同じ
IF (ASSIPLAY && TFLAG:50) || (ASSIPLAY == 0 && TFLAG:50 == 0)
	;前回の調教が自慰だとフェラ自慰に
	IF PREVCOM == 3
		CALL COM_ABLE125
		SIF RESULT == 1
			JUMP COM125
	ENDIF
ENDIF

;前回と今回の調教者が同じ
IF (ASSIPLAY && TFLAG:50) || (ASSIPLAY == 0 && TFLAG:50 == 0)
	;前回の調教がフェラかパイズリフェラかフェラ自慰か手コキフェラだとランダムでディープスロートかバキュームフェラに
	IF PREVCOM == 31 || PREVCOM == 123 || PREVCOM == 125 || PREVCOM == 126
		A = RAND:11
		IF A > 5
			CALL COM_ABLE124
			SIF RESULT == 1
			JUMP COM124
		ELSE
			CALL COM_ABLE127
			SIF RESULT == 1
			JUMP COM127
		ENDIF
	ENDIF
ENDIF

;前回と今回の調教者が同じ
IF (ASSIPLAY && TFLAG:50) || (ASSIPLAY == 0 && TFLAG:50 == 0)
	;前回の調教が手淫だと手コキフェラに
	IF PREVCOM == 30
		CALL COM_ABLE126
		SIF RESULT == 1
			JUMP COM126
	ENDIF
ENDIF

;前回の調教が3Pのとき3Pへ
TFLAG:42 = 0
IF PREVCOM == 64
	CALL COM_ABLE64
	IF RESULT == 1
		TFLAG:42 = 1
		JUMP COM64
	ENDIF
;調教者が前回:조수で今回:主人か、前回:主人で今回:조수のとき
ELSEIF (ASSIPLAY && TFLAG:50 == 0) || (ASSIPLAY == 0 && TFLAG:50) && TEQUIP:89 == 0
	;前回の調教が正常位か後背位か後背位애널のときは3Pへ
	IF PREVCOM == 20 || PREVCOM == 21 || PREVCOM == 27
		CALL COM_ABLE64
		SIF RESULT == 1
			JUMP COM64
	ENDIF
ENDIF

PRINTL 펠라치오

;-------------------------------------------------
;実行できるかの判定
;-------------------------------------------------
A = 0
S = 0

;すべての命令に共通の要素を考慮
;(従順が高いと命令に従いやすいなど)
CALL COM_ORDER

;ABL:욕망
IF ABL:11
	SIF S
		PRINT  + 
	A += ABL:11 * 1
	PRINTS ABLNAME:11
	PRINTV 'LV,ABL:11,'(,ABL:11 * 1,')
	S = 1
ENDIF
;ABL:봉사정신
IF ABL:16
	SIF S
		PRINT  + 
	A += ABL:16 * 4
	PRINTS ABLNAME:16
	PRINTV 'LV,ABL:16,'(,ABL:16 * 4,')
	S = 1
ENDIF
;ABL:정액중독
IF ABL:32
	SIF S
		PRINT  + 
	A += ABL:32 * 3
	PRINTS ABLNAME:32
	PRINTV 'LV,ABL:32,'(,ABL:32 * 3,')
	S = 1
ENDIF

IF MARK:1
	SIF S
		PRINT  + 
	A += MARK:1 * 1
	PRINTS MARKNAME:1
	PRINTV 'LV,MARK:1,'(,MARK:1 * 1,')
	S = 1
ENDIF


;PALAM:욕정
IF PALAM:5 < PALAMLV:1
	L = 0
ELSEIF PALAM:5 < PALAMLV:2
	L = 1
ELSEIF PALAM:5 < PALAMLV:3
	L = 2
ELSEIF PALAM:5 < PALAMLV:4
	L = 3
ELSEIF PALAM:5 < PALAMLV:5
	L = 4
ELSE
	L = 5
ENDIF
IF L
	SIF S
		PRINT  + 
	A += L * 1
	PRINTS PALAMNAME:5
	PRINTV 'LV,L,'(,L * 1,')
	S = 1
ENDIF

;수줍음
IF TALENT:35
	PRINT  - 
	A -= 1
	PRINTS TALENTNAME:35
	PRINTV '(,1,')
	S = 1
ENDIF
;악취둔감
IF TALENT:61
	SIF S
		PRINT  + 
	A += 1
	PRINTS TALENTNAME:61
	PRINTV '(,1,')
	S = 1
ENDIF
;악취민감
IF TALENT:62
	PRINT  - 
	A -= 3
	PRINTS TALENTNAME:62
	PRINTV '(,3,')
	S = 1
ENDIF
;헌신적
IF TALENT:63
	SIF S
		PRINT  + 
	A += 6
	PRINTS TALENTNAME:63
	PRINTV '(,6,')
	S = 1
ENDIF
;쾌감을부정
IF TALENT:71
	PRINT  - 
	A -= 1
	PRINTS TALENTNAME:71
	PRINTV '(,1,')
	S = 1
ENDIF
;남성혐오
IF TALENT:82 && TALENT:PLAYER:122
	PRINT  - 
	A -= 12
	PRINTS TALENTNAME:82
	PRINTV '(,12,')
	S = 1
ENDIF
;애정
IF TALENT:85 && ASSIPLAY == 0
	SIF S
		PRINT  + 
	A += 5
	PRINTS TALENTNAME:85
	PRINTV '(,5,')
	S = 1
ENDIF
;調教者がふたなり
IF TALENT:PLAYER:121
	SIF S
		PRINT  + 
	A += 8
	PRINTS TALENTNAME:121
	PRINTV '(,8,')
	S = 1
ENDIF
;獣姦
IF TEQUIP:89 && TALENT:136 == 0
	SIF S
		PRINT  - 
	A -= 15
	PRINTS ITEMNAME:22
	PRINTV '(,15,')
	S = 1
ENDIF

Y = 0
;愛液の汚れ
SIF STAIN:PLAYER:2 & 1
	Y += 1
;精液の汚れ
SIF STAIN:PLAYER:2 & 4
	Y += 3
;애널の汚れ
SIF STAIN:PLAYER:2 & 8
	Y += 7
;母乳の汚れ
SIF STAIN:PLAYER:2 & 16
	Y += 1
;尿の汚れ
SIF STAIN:PLAYER:4 & 32
	Y += 3

;콜로세움・獣姦の場合は汚れ7で固定
SIF TEQUIP:89 || TEQUIP:55
	Y = 7

SIF TALENT:61
	Y /= 3
SIF TALENT:62
	Y *= 2

;더러움 있음
IF Y
	PRINT  - 
	A -= Y
	;악취둔감
	IF TALENT:61
		PRINT 더러움 있음、
		PRINTS TALENTNAME:61
	ELSEIF TALENT:62
		PRINT 더러움 있음、
		PRINTS TALENTNAME:62
	ELSE
		PRINT 더러움 있음
	ENDIF
	PRINTV '(,Y,')
	S = 1
ENDIF

;合計を表示(24以上で実行)
PRINT  = 
PRINTV A

V = 24
SIF A < V
	PRINT  < 
SIF A == V
	PRINT  = 
SIF A > V
	PRINT  > 
PRINT 실행값
PRINTV V

WAIT

;実行できない
SIF A < V
	RETURN 0

;SAVESTR:22 = 펠라치오
CALL TRAIN_MESSAGE_B

;-------------------------------------------------
;実行決定
;-------------------------------------------------
PRINTFORML %EXPNAME:22%＋１
EXP:22 += 1

;-------------------------------------------------
;ソースの計算
;-------------------------------------------------
IF TALENT:47
	LOSEBASE:0 += 10
	LOSEBASE:1 += 90
ELSE
	LOSEBASE:0 += 10
	LOSEBASE:1 += 150
ENDIF

SOURCE:13 = 1500
SOURCE:14 = 500

;上のほうで計算した汚れデータ
SOURCE:8 = Y*40 + 100

;ABL:奉仕精神をみる
IF ABL:16 == 0
	SOURCE:4 = 420
	SOURCE:5 = 150
	TIMES SOURCE:8 , 4.00
ELSEIF ABL:16 == 1
	SOURCE:4 = 500
	SOURCE:5 = 300
	TIMES SOURCE:8 , 2.50
ELSEIF ABL:16 == 2
	SOURCE:4 = 580
	SOURCE:5 = 600
	TIMES SOURCE:8 , 1.50
ELSEIF ABL:16 == 3
	SOURCE:4 = 660
	SOURCE:5 = 900
	TIMES SOURCE:8 , 1.00
ELSEIF ABL:16 == 4
	SOURCE:4 = 740
	SOURCE:5 = 1500
	TIMES SOURCE:8 , 0.50
ELSE
	SOURCE:4 = 820
	SOURCE:5 = 2200
	TIMES SOURCE:8 , 0.10
ENDIF


;ABL:技巧をみる
IF ABL:12 == 0
	TIMES SOURCE:4 , 0.50
	TIMES SOURCE:5 , 0.50
ELSEIF ABL:12 == 1
	TIMES SOURCE:4 , 0.80
	TIMES SOURCE:5 , 0.80
ELSEIF ABL:12 == 2
	TIMES SOURCE:4 , 1.00
	TIMES SOURCE:5 , 1.00
ELSEIF ABL:12 == 3
	TIMES SOURCE:4 , 1.20
	TIMES SOURCE:5 , 1.20
ELSEIF ABL:12 == 4
	TIMES SOURCE:4 , 1.50
	TIMES SOURCE:5 , 1.50
ELSE
	TIMES SOURCE:4 , 2.00
	TIMES SOURCE:5 , 2.00
ENDIF

;獣姦の場合はここで終了
IF TEQUIP:89
	;犬のペニスとファーストキス
	IF CFLAG:16 == -1
		TFLAG:13 = 1
		CFLAG:16 = 997
		;屈服刻印３に相当
		TFLAG:200 = 3
	ENDIF

	RETURN 1
ENDIF

;コロシアムの場合は조수以外はここで終了
IF TEQUIP:55
	IF ASSI > 0 && ASSIPLAY
	ELSE
		;ファーストキス
		IF CFLAG:16 == -1
			CFLAG:16 = 995
			TFLAG:13 = 1
		ENDIF

		RETURN 1
	ENDIF
ENDIF

;-------------------------------------------------
;사정ゲージチェック
;-------------------------------------------------
B = 0

;ABL:技巧をみる
IF ABL:12 == 0
	B = 1200
ELSEIF ABL:12 == 1
	B = 1700
ELSEIF ABL:12 == 2
	B = 2300
ELSEIF ABL:12 == 3
	B = 3000
ELSEIF ABL:12 == 4
	B = 3600
ELSE
	B = 4200
ENDIF

;ABL:従順をみる
IF ABL:10 == 0
	TIMES B , 0.80
ELSEIF ABL:10 == 1
	TIMES B , 0.90
ELSEIF ABL:10 == 2
	TIMES B , 1.00
ELSEIF ABL:10 == 3
	TIMES B , 1.10
ELSEIF ABL:10 == 4
	TIMES B , 1.20
ELSE
	TIMES B , 1.30
ENDIF

;ABL:奉仕技術をみる
IF ABL:13 == 0
	TIMES B , 0.50
ELSEIF ABL:13 == 1
	TIMES B , 0.80
ELSEIF ABL:13 == 2
	TIMES B , 1.20
ELSEIF ABL:13 == 3
	TIMES B , 1.50
ELSEIF ABL:13 == 4
	TIMES B , 1.80
ELSE
	TIMES B , 2.40
ENDIF

;ABL:精液中毒をみる
IF ABL:32 == 0
	TIMES B , 1.00
ELSEIF ABL:32 == 1
	TIMES B , 1.20
ELSEIF ABL:32 == 2
	TIMES B , 1.30
ELSEIF ABL:32 == 3
	TIMES B , 1.50
ELSEIF ABL:32 == 4
	TIMES B , 1.70
ELSE
	TIMES B , 2.00
ENDIF

;TALENT:舌使いをみる
SIF TALENT:52
	TIMES B , 2.00

;プレイヤーのABL:Ｃ感覚をみる
IF ABL:PLAYER:0 == 0
	TIMES B , 1.00
ELSEIF ABL:PLAYER:0 == 1
	TIMES B , 1.50
ELSEIF ABL:PLAYER:0 == 2
	TIMES B , 2.00
ELSEIF ABL:PLAYER:0 == 3
	TIMES B , 2.50
ELSEIF ABL:PLAYER:0 == 4
	TIMES B , 3.50
ELSE
	TIMES B , 5.00
ENDIF

SIF TALENT:PLAYER:119 || TALENT:PLAYER:122  || TALENT:PLAYER:121
	BASE:PLAYER:2 += B

;-------------------------------------------------
;사정チェック
;-------------------------------------------------
S = BASE:PLAYER:2
EJAC = MAXBASE:PLAYER:2

IF  S > EJAC * 2
	E = 2
ELSEIF S > EJAC
	E = 1
ELSE
	E = 0
ENDIF

IF E
;사정している
	TIMES SOURCE:4 , 3.00

	;ABL:精液中毒をみる
	IF ABL:32 == 0
		SOURCE:7 = 0
		TIMES SOURCE:5 , 2.00
		TIMES SOURCE:13, 4.00
	ELSEIF ABL:32 == 1
		SOURCE:7 = 500
		TIMES SOURCE:5 , 3.00
		TIMES SOURCE:13, 3.00
	ELSEIF ABL:32 == 2
		SOURCE:7 = 1200
		TIMES SOURCE:5 , 4.00
		TIMES SOURCE:13, 2.50
	ELSEIF ABL:32 == 3
		SOURCE:7 = 3000
		TIMES SOURCE:5 , 6.00
		TIMES SOURCE:13, 2.00
	ELSEIF ABL:32 == 4
		SOURCE:7 = 6000
		TIMES SOURCE:5 , 9.00
		TIMES SOURCE:13, 1.50
	ELSE
		SOURCE:7 = 12000
		TIMES SOURCE:5 , 15.00
		TIMES SOURCE:13, 1.00
	ENDIF
ENDIF

;대량사정
IF E == 2
	TIMES SOURCE:7 , 2.00
	TIMES SOURCE:5 , 1.50

	EXP:PLAYER:3 += 2
	EXP:20 += 9
	PRINTL 대량사정
	PRINTL 정액경험＋９
	;精液汚れ
	STAIN:PLAYER:2 |= 4

	BASE:PLAYER:2 -= EJAC*2
	SIF BASE:PLAYER:2 >= EJAC
		BASE:PLAYER:2 = EJAC-1

	;口で사정させたフラグ
	TFLAG:0 = 2
;通常の사정
ELSEIF E == 1
	EXP:PLAYER:3 += 1
	EXP:20 += 3
	PRINTL 사정
	PRINTL 정액경험＋３
	;精液汚れ
	STAIN:PLAYER:2 |= 4

	BASE:PLAYER:2 -= EJAC
	SIF BASE:PLAYER:2 >= EJAC
		BASE:PLAYER:2 = EJAC-1

	;口で사정させたフラグ
	TFLAG:0 = 1
ENDIF

;-------------------------------------------------
;汚れ
;-------------------------------------------------
;노예の口⇔調教者のＰの汚れが移動
STAIN:0 |= STAIN:PLAYER:2
STAIN:PLAYER:2 |= STAIN:0

;奉仕精神ＬＶ２以上、技巧ＬＶ２以上なら汚れをなめ取る
IF ABL:16 >= 2 && ABL:12 >= 2
	STAIN:PLAYER:2 = 2
	SIF E >= 1
		TFLAG:8 = 1
ENDIF

;-------------------------------------------------
;経験上昇
;-------------------------------------------------
IF TALENT:122 == 0 && TALENT:PLAYER:122 == 0
	PRINTS EXPNAME:40
	PRINTL +7
	EXP:40 += 7
ELSEIF TALENT:122 == 1 && TALENT:PLAYER:122 == 1
	PRINTS EXPNAME:41
	PRINTL +7
	EXP:41 += 7
ENDIF

SIF ASSIPLAY == 0 && EXP:0 >= EXPLV:3
		TFLAG:30 += 1

IF TALENT:122
	E = 2
ELSE
	E = 1
ENDIF
;ファーストキス
IF CFLAG:16 == -1
	CFLAG:16 = NO:PLAYER + 201
	CSTR:4 = %SAVESTR:PLAYER%
	TFLAG:13 = 1
ENDIF
;애정경험
IF CFLAG:2 >= 1000 && ASSIPLAY == 0
	PRINTFORML %EXPNAME:23%+{E}
	EXP:23 += E
ENDIF
E = 0

;調教者がふたなり
IF TALENT:PLAYER:121
	SOURCE:13 /= 2
ENDIF

TFLAG:100 = 1

;屈服刻印２に相当
TFLAG:200 = 2

RETURN 1
