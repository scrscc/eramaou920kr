;eraIM@Sから導入しました(eramaou)

;-------------------------------------------------
;정상위・가슴애무
;セックス系コマンド：정상위＋胸愛撫の効果
;-------------------------------------------------
@COM129

SELECTCOM = 129

;処女を奪うかどうかの確認
CALL CONFIRM_LOST_VIRGIN
SIF RESULT == 0
	RETURN 0

;コンドームを使うかの確認
CALL CONFIRM_CONDOM
SIF !RESULT
	RETURN 0
	
PRINTL 정상위・가슴애무
;SAVESTR:22 = 정상위・가슴애무
CALL TRAIN_MESSAGE_B

;V経験を伴うコマンドのフラグ
TFLAG:19 = 1
SIF TALENT:85 && ASSIPLAY == 0 && EXP:0 == 0
	TFLAG:20 = 1

;調教者の사정チェック
CALL COM_EJAC_PLAYER_SEX

;-------------------------------------------------
;ソースの計算
;-------------------------------------------------
LOSEBASE:0 += 60
LOSEBASE:1 += 120

SOURCE:12 = 400

;ABL:Ｖ感覚をみる
IF ABL:2 == 0
	SOURCE:1 = 40
	SOURCE:3 = 150
ELSEIF ABL:2 == 1
	SOURCE:1 = 150
	SOURCE:3 = 250
ELSEIF ABL:2 == 2
	SOURCE:1 = 400
	SOURCE:3 = 350
ELSEIF ABL:2 == 3
	SOURCE:1 = 1000
	SOURCE:3 = 500
ELSEIF ABL:2 == 4
	SOURCE:1 = 1700
	SOURCE:3 = 700
ELSE
	SOURCE:1 = 2200
	SOURCE:3 = 1000
ENDIF

;EXP:Ｖ経験をみる
IF EXP:0 < EXPLV:1
	TIMES SOURCE:1 , 0.20
	SOURCE:6 = 5500
ELSEIF EXP:0 < EXPLV:2
	TIMES SOURCE:1 , 0.60
	SOURCE:6 = 300
ELSEIF EXP:0 < EXPLV:3
	TIMES SOURCE:1 , 1.00
	SOURCE:6 = 50
ELSEIF EXP:0 < EXPLV:4
	TIMES SOURCE:1 , 1.20
	SOURCE:6 = 10
ELSEIF EXP:0 < EXPLV:5
	TIMES SOURCE:1 , 1.30
	SOURCE:6 = 0
ELSE
	TIMES SOURCE:1 , 1.80
	SOURCE:6 = 0
ENDIF

;ABL:Ｂ感覚をみる
IF ABL:1 == 0
	SOURCE:17 = 50
	SOURCE:3 += 50
ELSEIF ABL:1 == 1
	SOURCE:17 = 200
	SOURCE:3 += 100
ELSEIF ABL:1 == 2
	SOURCE:17 = 500
	SOURCE:3 += 200
ELSEIF ABL:1 == 3
	SOURCE:17 = 800
	SOURCE:3 += 300
ELSEIF ABL:1 == 4
	SOURCE:17 = 1300
	SOURCE:3 += 500
ELSE
	SOURCE:17 = 1800
	SOURCE:3 += 700
ENDIF

;PALAM:潤滑をみる
IF PALAM:3 < PALAMLV:1
	TIMES SOURCE:1 , 0.10
	SOURCE:6 += 1000
	TIMES SOURCE:6 , 3.00
ELSEIF PALAM:3 < PALAMLV:2
	TIMES SOURCE:1 , 0.40
	SOURCE:6 += 300
	TIMES SOURCE:6 , 1.00
ELSEIF PALAM:3 < PALAMLV:3
	TIMES SOURCE:1 , 1.00
	TIMES SOURCE:6 , 0.50
ELSEIF PALAM:3 < PALAMLV:4
	TIMES SOURCE:1 , 1.40
	TIMES SOURCE:6 , 0.20
ELSEIF PALAM:3 >= PALAMLV:4
	TIMES SOURCE:1 , 1.80
	TIMES SOURCE:6 , 0.10
ENDIF

;調教者がふたなり
IF ASSIPLAY
	SIF TALENT:ASSI:121
		TIMES SOURCE:1 , 2.50
ENDIF

;장신
SIF TALENT:99
	TIMES SOURCE:6 , 0.80
;小柄体形
SIF TALENT:100
	TIMES SOURCE:6 , 2.00
;미숙
SIF TALENT:135
	TIMES SOURCE:6 , 4.00

;정조관념
IF TALENT:30
	IF EXP:0 == 0
		TIMES SOURCE:3 , 0.60
		SOURCE:15 = 10000
	ELSE
		TIMES SOURCE:3 , 0.60
		SOURCE:15 = 1000
	ENDIF
;정조관부실
ELSEIF TALENT:31
	IF EXP:0 == 0
		TIMES SOURCE:3 , 0.60
		SOURCE:15 = 300
	ENDIF
ELSE
	IF EXP:0 == 0
		SOURCE:15 = 3000
	ENDIF
ENDIF

;PALAM:欲情をみる
IF PALAM:5 < PALAMLV:1
	TIMES SOURCE:1 , 0.60
	TIMES SOURCE:3 , 0.30
ELSEIF PALAM:5 < PALAMLV:2
	TIMES SOURCE:1 , 0.80
	TIMES SOURCE:3 , 0.60
ELSEIF PALAM:5 < PALAMLV:3
	TIMES SOURCE:1 , 1.00
	TIMES SOURCE:3 , 1.00
ELSEIF PALAM:5 < PALAMLV:4
	TIMES SOURCE:1 , 1.20
	TIMES SOURCE:3 , 1.50
ELSEIF PALAM:5 >= PALAMLV:4
	TIMES SOURCE:1 , 1.50
	TIMES SOURCE:3 , 1.80
ENDIF

;ABL:従順をみる
IF ABL:10 == 0
	TIMES SOURCE:1 , 0.50
	TIMES SOURCE:3 , 0.60
	TIMES SOURCE:15 , 2.00
ELSEIF ABL:10 == 1
	TIMES SOURCE:1 , 0.80
	TIMES SOURCE:3 , 0.80
	TIMES SOURCE:15 , 1.50
ELSEIF ABL:10 == 2
	TIMES SOURCE:1 , 1.00
	TIMES SOURCE:3 , 1.00
	TIMES SOURCE:15 , 1.00
ELSEIF ABL:10 == 3
	TIMES SOURCE:1 , 1.30
	TIMES SOURCE:3 , 1.20
	TIMES SOURCE:15 , 0.80
ELSEIF ABL:10 == 4
	TIMES SOURCE:1 , 1.60
	TIMES SOURCE:3 , 1.40
	TIMES SOURCE:15 , 0.60
ELSE
	TIMES SOURCE:1 , 2.00
	TIMES SOURCE:3 , 1.60
	TIMES SOURCE:15 , 0.30
ENDIF

;セックス後の処理
CALL COM_AFTER_VAGINA_SEX

RETURN 1
;
