;eraIM@Sから導入しました(eramaou)

;-------------------------------------------------
;거품문지르기
;奉仕系コマンド：調教対象が調教者のペニスをクリトリスとヴァギナで刺激
;主に屈従と逸脱を上げる
;-------------------------------------------------
@COM35

PRINTL 거품문지르기

;-------------------------------------------------
;実行できるかの判定
;-------------------------------------------------
A = 0
S = 0

;すべての命令に共通の要素を考慮
;(従順が高いと命令に従いやすいなど)
CALL COM_ORDER

;ABL:욕망
IF ABL:11
	SIF S
		PRINT  + 
	A += ABL:11 * 1
	PRINTS ABLNAME:11
	PRINTV 'LV,ABL:11,'(,ABL:11 * 1,')
	S = 1
ENDIF
;ABL:봉사정신
IF ABL:16
	SIF S
		PRINT  + 
	A += ABL:16 * 4
	PRINTS ABLNAME:16
	PRINTV 'LV,ABL:16,'(,ABL:16 * 4,')
	S = 1
ENDIF
;ABL:정액중독
IF ABL:32
	SIF S
		PRINT  + 
	A += ABL:32 * 3
	PRINTS ABLNAME:32
	PRINTV 'LV,ABL:32,'(,ABL:32 * 3,')
	S = 1
ENDIF

IF MARK:1
	SIF S
		PRINT  + 
	A += MARK:1 * 1
	PRINTS MARKNAME:1
	PRINTV 'LV,MARK:1,'(,MARK:1 * 1,')
	S = 1
ENDIF


;PALAM:욕정
IF PALAM:5 < PALAMLV:1
	L = 0
ELSEIF PALAM:5 < PALAMLV:2
	L = 1
ELSEIF PALAM:5 < PALAMLV:3
	L = 2
ELSEIF PALAM:5 < PALAMLV:4
	L = 3
ELSEIF PALAM:5 < PALAMLV:5
	L = 4
ELSE
	L = 5
ENDIF
IF L
	SIF S
		PRINT  + 
	A += L * 1
	PRINTS PALAMNAME:5
	PRINTV 'LV,L,'(,L * 1,')
	S = 1
ENDIF

;수줍음
IF TALENT:35
	PRINT  - 
	A -= 3
	PRINTS TALENTNAME:35
	PRINTV '(,3,')
	S = 1
ENDIF
;악취둔감
IF TALENT:61
	SIF S
		PRINT  + 
	A += 1
	PRINTS TALENTNAME:61
	PRINTV '(,1,')
	S = 1
ENDIF
;악취민감
IF TALENT:62
	PRINT  - 
	A -= 3
	PRINTS TALENTNAME:62
	PRINTV '(,3,')
	S = 1
ENDIF
;헌신적
IF TALENT:63
	SIF S
		PRINT  + 
	A += 6
	PRINTS TALENTNAME:63
	PRINTV '(,6,')
	S = 1
ENDIF
;쾌감을부정
IF TALENT:71
	PRINT  - 
	A -= 3
	PRINTS TALENTNAME:71
	PRINTV '(,3,')
	S = 1
ENDIF
;남성혐오
IF TALENT:82 && TALENT:PLAYER:122
	PRINT  - 
	A -= 12
	PRINTS TALENTNAME:82
	PRINTV '(,12,')
	S = 1
ENDIF
;애정
IF TALENT:85 && ASSIPLAY == 0
	SIF S
		PRINT  + 
	A += 5
	PRINTS TALENTNAME:85
	PRINTV '(,5,')
	S = 1
ENDIF

;調教者がふたなり
IF TALENT:PLAYER:121
	SIF S
		PRINT  + 
	A += 8
	PRINTS TALENTNAME:121
	PRINTV '(,8,')
	S = 1
ENDIF

Y = 0
;愛液の汚れ
SIF STAIN:PLAYER:2 & 1
	Y += 1
;精液の汚れ
SIF STAIN:PLAYER:2 & 4
	Y += 3
;애널の汚れ
SIF STAIN:PLAYER:2 & 8
	Y += 7
;母乳の汚れ
SIF STAIN:PLAYER:2 & 16
	Y += 1
;尿の汚れ
SIF STAIN:PLAYER:4 & 32
	Y += 3

SIF TALENT:61
	Y /= 3
SIF TALENT:62
	Y *= 2
;더러움 있음
IF Y
	PRINT  - 
	A -= Y
	;악취둔감
	IF TALENT:61
		PRINT 더러움 있음、;이하 원문은 더러움 있음
		PRINTS TALENTNAME:61
	ELSEIF TALENT:62
		PRINT 더러움 있음、
		PRINTS TALENTNAME:62
	ELSE
		PRINT 더러움 있음
	ENDIF
	PRINTV '(,Y,')
	S = 1
ENDIF

;合計を表示(30以上で実行)
PRINT  = 
PRINTV A

V = 30
SIF A < V
	PRINT  < 
SIF A == V
	PRINT  = 
SIF A > V
	PRINT  > 
PRINT 실행값
PRINTV V

WAIT

;実行できない
SIF A < V
	RETURN 0

;SAVESTR:22 = 거품문지르기
CALL TRAIN_MESSAGE_B

;-------------------------------------------------
;実行決定
;-------------------------------------------------
;-------------------------------------------------
;ソースの計算
;-------------------------------------------------
LOSEBASE:0 += 50
LOSEBASE:1 += 180

SOURCE:13 = 2000
SOURCE:14 = 1500

;ABL:奉仕精神をみる
IF ABL:16 == 0
	SOURCE:4 = 500
	SOURCE:5 = 300
	SOURCE:8 = 200
ELSEIF ABL:16 == 1
	SOURCE:4 = 700
	SOURCE:5 = 500
	SOURCE:8 = 150
ELSEIF ABL:16 == 2
	SOURCE:4 = 900
	SOURCE:5 = 800
	SOURCE:8 = 100
ELSEIF ABL:16 == 3
	SOURCE:4 = 1100
	SOURCE:5 = 1200
	SOURCE:8 = 50
ELSEIF ABL:16 == 4
	SOURCE:4 = 1300
	SOURCE:5 = 1800
	SOURCE:8 = 20
ELSE
	SOURCE:4 = 1500
	SOURCE:5 = 2500
	SOURCE:8 = 0
ENDIF

;ABL:Ｃ感覚をみる
IF ABL:0 == 0
	SOURCE:0 = 0
ELSEIF ABL:0 == 1
	SOURCE:0 = 50
ELSEIF ABL:0 == 2
	SOURCE:0 = 200
ELSEIF ABL:0 == 3
	SOURCE:0 = 400
ELSEIF ABL:0 == 4
	SOURCE:0 = 1000
ELSE
	SOURCE:0 = 2000
ENDIF

;ABL:Ｂ感覚をみる
IF ABL:1 == 0
	A = 200
ELSEIF ABL:1 == 1
	A = 300
ELSEIF ABL:1 == 2
	A = 500
ELSEIF ABL:1 == 3
	A = 1000
ELSEIF ABL:1 == 4
	A = 1500
ELSE
	A = 1800
ENDIF

IF TALENT:110
	TIMES A , 1.20
ENDIF
IF TALENT:108
	TIMES A , 1.20
ELSEIF TALENT:107
	TIMES A , 0.70
ENDIF

SOURCE:17 += A
;ABL:技巧をみる
IF ABL:12 == 0
	TIMES SOURCE:4 , 0.80
	TIMES SOURCE:5 , 0.80
ELSEIF ABL:12 == 1
	TIMES SOURCE:4 , 1.00
	TIMES SOURCE:5 , 1.00
ELSEIF ABL:12 == 2
	TIMES SOURCE:4 , 1.30
	TIMES SOURCE:5 , 1.30
ELSEIF ABL:12 == 3
	TIMES SOURCE:4 , 1.60
	TIMES SOURCE:5 , 1.60
ELSEIF ABL:12 == 4
	TIMES SOURCE:4 , 1.90
	TIMES SOURCE:5 , 1.90
ELSE
	TIMES SOURCE:4 , 2.30
	TIMES SOURCE:5 , 2.30
ENDIF
;-------------------------------------------------
;사정ゲージチェック
;-------------------------------------------------
B = 0

;ABL:技巧をみる
IF ABL:12 == 0
	B = 1500
ELSEIF ABL:12 == 1
	B = 2100
ELSEIF ABL:12 == 2
	B = 2900
ELSEIF ABL:12 == 3
	B = 4000
ELSEIF ABL:12 == 4
	B = 5000
ELSE
	B = 6000
ENDIF

;ABL:従順をみる
IF ABL:10 == 0
	TIMES B , 0.80
ELSEIF ABL:10 == 1
	TIMES B , 0.90
ELSEIF ABL:10 == 2
	TIMES B , 1.00
ELSEIF ABL:10 == 3
	TIMES B , 1.10
ELSEIF ABL:10 == 4
	TIMES B , 1.20
ELSE
	TIMES B , 1.30
ENDIF

;ABL:奉仕精神をみる
IF ABL:16 == 0
	TIMES B , 0.50
ELSEIF ABL:16 == 1
	TIMES B , 0.80
ELSEIF ABL:16 == 2
	TIMES B , 1.20
ELSEIF ABL:16 == 3
	TIMES B , 1.50
ELSEIF ABL:16 == 4
	TIMES B , 1.80
ELSE
	TIMES B , 2.40
ENDIF

;ABL:精液中毒をみる
IF ABL:32 == 0
	TIMES B , 0.80
ELSEIF ABL:32 == 1
	TIMES B , 0.90
ELSEIF ABL:32 == 2
	TIMES B , 1.00
ELSEIF ABL:32 == 3
	TIMES B , 1.10
ELSEIF ABL:32 == 4
	TIMES B , 1.20
ELSE
	TIMES B , 1.30
ENDIF

;プレイヤーのABL:Ｃ感覚をみる
IF ABL:PLAYER:0 == 0
	TIMES B , 1.00
ELSEIF ABL:PLAYER:0 == 1
	TIMES B , 1.50
ELSEIF ABL:PLAYER:0 == 2
	TIMES B , 2.00
ELSEIF ABL:PLAYER:0 == 3
	TIMES B , 2.50
ELSEIF ABL:PLAYER:0 == 4
	TIMES B , 3.50
ELSE
	TIMES B , 5.00
ENDIF

SIF TALENT:PLAYER:121 || TALENT:PLAYER:122
	BASE:PLAYER:2 += B

;-------------------------------------------------
;사정チェック
;-------------------------------------------------
S = BASE:PLAYER:2
EJAC = MAXBASE:PLAYER:2

IF  S > EJAC * 2
	E = 2
ELSEIF S > EJAC
	E = 1
ELSE
	E = 0
ENDIF

IF E
;사정している
	TIMES SOURCE:4 , 3.00

	;ABL:精液中毒をみる
	IF ABL:32 == 0
		SOURCE:7 = 0
		TIMES SOURCE:5 , 2.00
		TIMES SOURCE:13, 2.00
	ELSEIF ABL:32 == 1
		SOURCE:7 = 200
		TIMES SOURCE:5 , 2.50
		TIMES SOURCE:13, 1.60
	ELSEIF ABL:32 == 2
		SOURCE:7 = 600
		TIMES SOURCE:5 , 3.00
		TIMES SOURCE:13, 1.00
	ELSEIF ABL:32 == 3
		SOURCE:7 = 1500
		TIMES SOURCE:5 , 4.00
		TIMES SOURCE:13, 0.70
	ELSEIF ABL:32 == 4
		SOURCE:7 = 3000
		TIMES SOURCE:5 , 5.00
		TIMES SOURCE:13, 0.40
	ELSE
		SOURCE:7 = 6000
		TIMES SOURCE:5 , 6.00
		TIMES SOURCE:13, 0.10
	ENDIF
ENDIF

;대량사정
IF E == 2
	TIMES SOURCE:7 , 2.00
	TIMES SOURCE:5 , 1.50

	EXP:PLAYER:3 += 2
	EXP:20 += 3
	PRINTL 대량사정
	PRINTL 정액경험＋３
	;精液汚れ
	STAIN:PLAYER:2 |= 4

	BASE:PLAYER:2 -= EJAC*2
	SIF BASE:PLAYER:2 >= EJAC
		BASE:PLAYER:2 = EJAC-1

	;手で사정させたフラグ
	TFLAG:1 = 2
;通常の사정
ELSEIF E == 1
	EXP:PLAYER:3 += 1
	EXP:20 += 1
	PRINTL 사정
	PRINTL 정액경험＋１

	;精液汚れ
	STAIN:PLAYER:2 |= 4

	BASE:PLAYER:2 -= EJAC
	SIF BASE:PLAYER:2 >= EJAC
		BASE:PLAYER:2 = EJAC-1

	;手で사정させたフラグ
	TFLAG:1 = 1
ENDIF

;-------------------------------------------------
;汚れ
;-------------------------------------------------
STAIN:0 = 0
STAIN:1 = 0
STAIN:2 = 2
STAIN:3 = 1
STAIN:4 = 8

IF ASSIPLAY == 0
	STAIN:MASTER:0 = 0
	STAIN:MASTER:1 = 0
	STAIN:MASTER:2 = 2
	STAIN:MASTER:3 = 1
	STAIN:MASTER:4 = 8
ELSEIF ASSIPLAY
	STAIN:ASSI:0 = 0
	STAIN:ASSI:1 = 0
	STAIN:ASSI:2 = 2
	STAIN:ASSI:3 = 1
	STAIN:ASSI:4 = 8
ENDIF

;-------------------------------------------------
;経験上昇
;-------------------------------------------------
IF TALENT:122 == 0 && TALENT:PLAYER:122 == 0
	PRINTS EXPNAME:40
	PRINTL +7
	EXP:40 += 7
ELSEIF TALENT:122 == 1 && TALENT:PLAYER:122 == 1
	PRINTS EXPNAME:41
	PRINTL +7
	EXP:41 += 7
ENDIF

SIF ASSIPLAY == 0 && EXP:22 >= EXPLV:3
	TFLAG:30 += 1

;애정경험
E = 2
IF CFLAG:2 >= 1000 && ASSIPLAY == 0
	PRINTFORML %EXPNAME:23%+{E}
	EXP:23 += E
ENDIF
E = 0

TFLAG:100 = 1

;屈服刻印３に相当
TFLAG:200 = 3

;調教者がふたなり
IF TALENT:PLAYER:121
	SOURCE:13 /= 2
ENDIF

RETURN 1
