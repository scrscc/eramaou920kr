;eraIM@Sから導入しました(eramaou)
;それを正常位を乳用に改造

;-------------------------------------------------
;ニプルファック
;ハード系コマンド：調教者のペニスを調教対象の乳に挿入
;-------------------------------------------------
@COM90

;コンドームを使うかの確認
CALL CONFIRM_CONDOM
SIF !RESULT
	RETURN 0


PRINTL 니플 퍽
CALL TRAIN_MESSAGE_B

;Ｂ経験を伴うコマンドのフラグ
TFLAG:19 = 1
;SIF ASSIPLAY == 0 && EXP:35 == 0 && TEQUIP:89 == 0
;	TFLAG:20 = 1

;乳房挿入フラグ
CFLAG:113 = 1

;調教者の射精チェック
CALL COM_EJAC_PLAYER_SEX

;-------------------------------------------------
;ソースの計算
;-------------------------------------------------
LOSEBASE:0 += 50
LOSEBASE:1 += 100

SOURCE:12 = 400

;ABL:Ｂ感覚をみる
IF ABL:1 == 0
	SOURCE:17 = 40
	SOURCE:3 = 150
ELSEIF ABL:1 == 1
	SOURCE:17 = 150
	SOURCE:3 = 250
ELSEIF ABL:1 == 2
	SOURCE:17 = 400
	SOURCE:3 = 350
ELSEIF ABL:1 == 3
	SOURCE:17 = 1000
	SOURCE:3 = 500
ELSEIF ABL:1 == 4
	SOURCE:17 = 1700
	SOURCE:3 = 700
ELSE
	SOURCE:17 = 2200
	SOURCE:3 = 1000
ENDIF

;EXP:Ｂ経験をみる
IF EXP:35 < EXPLV:1
	TIMES SOURCE:17 , 0.20
	SOURCE:6 = 5500
ELSEIF EXP:35 < EXPLV:2
	TIMES SOURCE:17 , 0.60
	SOURCE:6 = 300
ELSEIF EXP:35 < EXPLV:3
	TIMES SOURCE:17 , 1.00
	SOURCE:6 = 50
ELSEIF EXP:35 < EXPLV:4
	TIMES SOURCE:17 , 1.20
	SOURCE:6 = 10
ELSEIF EXP:35 < EXPLV:5
	TIMES SOURCE:17 , 1.30
	SOURCE:6 = 0
ELSE
	TIMES SOURCE:17 , 1.80
	SOURCE:6 = 0
ENDIF

;PALAM:潤滑をみる
IF PALAM:3 < PALAMLV:1
	TIMES SOURCE:17 , 0.10
	SOURCE:6 += 1000
	TIMES SOURCE:6 , 3.00
ELSEIF PALAM:3 < PALAMLV:2
	TIMES SOURCE:17 , 0.40
	SOURCE:6 += 300
	TIMES SOURCE:6 , 1.00
ELSEIF PALAM:3 < PALAMLV:3
	TIMES SOURCE:17 , 1.00
	TIMES SOURCE:6 , 0.50
ELSEIF PALAM:3 < PALAMLV:4
	TIMES SOURCE:17 , 1.40
	TIMES SOURCE:6 , 0.20
ELSEIF PALAM:3 >= PALAMLV:4
	TIMES SOURCE:17 , 1.80
	TIMES SOURCE:6 , 0.10
ENDIF

;調教者がふたなり
IF ASSIPLAY
	SIF TALENT:ASSI:121
		TIMES SOURCE:17 , 2.50
ENDIF

;장신
SIF TALENT:99
	TIMES SOURCE:6 , 0.80
;小柄体形
SIF TALENT:100
	TIMES SOURCE:6 , 2.00
;미숙
SIF TALENT:135
	TIMES SOURCE:6 , 4.00

;PALAM:欲情をみる
IF PALAM:5 < PALAMLV:1
	TIMES SOURCE:17 , 0.60
	TIMES SOURCE:3 , 0.30
ELSEIF PALAM:5 < PALAMLV:2
	TIMES SOURCE:17 , 0.80
	TIMES SOURCE:3 , 0.60
ELSEIF PALAM:5 < PALAMLV:3
	TIMES SOURCE:17 , 1.00
	TIMES SOURCE:3 , 1.00
ELSEIF PALAM:5 < PALAMLV:4
	TIMES SOURCE:17 , 1.20
	TIMES SOURCE:3 , 1.50
ELSEIF PALAM:5 >= PALAMLV:4
	TIMES SOURCE:17 , 1.50
	TIMES SOURCE:3 , 1.80
ENDIF

;ABL:従順をみる
IF ABL:10 == 0
	TIMES SOURCE:17 , 0.50
	TIMES SOURCE:3 , 0.60
	TIMES SOURCE:15 , 2.00
ELSEIF ABL:10 == 1
	TIMES SOURCE:17 , 0.80
	TIMES SOURCE:3 , 0.80
	TIMES SOURCE:15 , 1.50
ELSEIF ABL:10 == 2
	TIMES SOURCE:17 , 1.00
	TIMES SOURCE:3 , 1.00
	TIMES SOURCE:15 , 1.00
ELSEIF ABL:10 == 3
	TIMES SOURCE:17 , 1.30
	TIMES SOURCE:3 , 1.20
	TIMES SOURCE:15 , 0.80
ELSEIF ABL:10 == 4
	TIMES SOURCE:17 , 1.60
	TIMES SOURCE:3 , 1.40
	TIMES SOURCE:15 , 0.60
ELSE
	TIMES SOURCE:17 , 2.00
	TIMES SOURCE:3 , 1.60
	TIMES SOURCE:15 , 0.30
ENDIF

;セックス後の処理
CALL COM_AFTER_EXTRA_SEX

RETURN 1
;
