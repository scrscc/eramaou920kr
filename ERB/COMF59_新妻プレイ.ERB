@COM59
;새색시플레이

PRINTL 새색시플레이
;SAVESTR:22 = 새색시플레이
CALL TRAIN_MESSAGE_B

;エプロンのセット
;終了時は修正なし
IF TEQUIP:59
	TEQUIP:59 = 0
	RETURN 1
ENDIF

LOSEBASE:0 += 0
LOSEBASE:1 += 30


A = 500
B = 100

;PALAM:欲情をみる
IF PALAM:5 < PALAMLV:1
	TIMES A , 1.00
ELSEIF PALAM:5 < PALAMLV:2
	TIMES A , 1.10
ELSEIF PALAM:5 < PALAMLV:3
	TIMES A , 1.20
ELSEIF PALAM:5 < PALAMLV:4
	TIMES A , 1.30
ELSEIF PALAM:5 >= PALAMLV:4
	TIMES A , 1.40
ENDIF

;ABL:露出癖をみる
IF ABL:17 == 0
	SOURCE:11 += 0
	SOURCE:10 += 0
	SOURCE:12 += 100
	SOURCE:14 += 100
	SOURCE:15 += 100
	TIMES A, 0.60
ELSEIF ABL:17 == 1
	SOURCE:11 += 50
	SOURCE:10 += 150
	SOURCE:12 += 500
	SOURCE:14 += 300
	TIMES A, 1.00
ELSEIF ABL:17 == 2
	SOURCE:11 += 100
	SOURCE:10 += 300
	SOURCE:12 += 100
	SOURCE:14 += 50
	TIMES A, 1.60
ELSEIF ABL:17 == 3
	SOURCE:11 += 150
	SOURCE:10 += 600
	SOURCE:12 += 50
	SOURCE:14 += 0
	TIMES A, 2.00
ELSEIF ABL:17 == 4
	SOURCE:11 += 200
	SOURCE:10 += 1000
	SOURCE:12 += 0
	SOURCE:14 += 0
	TIMES A, 2.60
ELSE
	SOURCE:11 += 300
	SOURCE:10 += 2000
	SOURCE:12 += 0
	SOURCE:14 += 0
	TIMES A, 3.80
ENDIF
;ABL:従順をみる
IF ABL:10 == 0
	TIMES A , 1.00
ELSEIF ABL:10 == 1
	TIMES A , 1.20
ELSEIF ABL:10 == 2
	TIMES A , 1.40
ELSEIF ABL:10 == 3
	TIMES A , 1.60
ELSEIF ABL:10 == 4
	TIMES A , 1.80
ELSE
	TIMES A , 2.00
ENDIF

;ABL:奉仕精神をみる
IF ABL:16 == 0
	TIMES A , 1.00
ELSEIF ABL:16 == 1
	TIMES A , 1.20
ELSEIF ABL:16 == 2
	TIMES A , 1.40
ELSEIF ABL:16 == 3
	TIMES A , 1.60
ELSEIF ABL:16 == 4
	TIMES A , 1.80
ELSE
	TIMES A , 2.00
ENDIF

;무관심
SIF TALENT:21
	TIMES A , 0.80
;감정부족
SIF TALENT:22
	TIMES A , 0.80
;호기심
SIF TALENT:23
	TIMES A , 1.50
;헌신적
SIF TALENT:63
	TIMES A , 1.50
;겁쟁이
SIF TALENT:10
	TIMES B , 1.50
;수줍음
SIF TALENT:35
	TIMES B , 1.20
;정조관념
SIF TALENT:33
	TIMES A , 0.50
;프라이드높음
SIF TALENT:15
	TIMES B , 2.00
;恋慕
SIF TALENT:85
	TIMES A , 2.50
;음란
SIF TALENT:76
	TIMES A , 1.80

SOURCE:3 += A
SOURCE:14 += B
SOURCE:16 += A

TEQUIP:59 = 1

SIF ASSIPLAY == 0 && ABL:10>= 3
	TFLAG:30 += 10
SIF ASSIPLAY == 0 && TALENT:TARGET:85
	TFLAG:30 += 20
SIF ASSIPLAY == 0 && TALENT:TARGET:76
	TFLAG:30 += 5
SIF ASSIPLAY == 0 && TALENT:TARGET:88
	TFLAG:30 += 30

;애정경험
E = 2
IF CFLAG:2 >= 1000 && ASSIPLAY == 0
	PRINTFORML %EXPNAME:23%+{E}
	EXP:23 += E
ENDIF
E = 0

RETURN 1


