;eraIM@Sから導入しました(eramaou)

;-------------------------------------------------
;투구맞대기
;-------------------------------------------------
@COM122

PRINTL 투구맞대기

;-------------------------------------------------
;実行できるかの判定
;-------------------------------------------------
A = 0
S = 0

;すべての命令に共通の要素を考慮
;(従順が高いと命令に従いやすいなど)
CALL COM_ORDER

;ABL:욕망
IF ABL:11
	SIF S
		PRINT  + 
	A += ABL:11 * 2
	PRINTS ABLNAME:11
	PRINTV 'LV,ABL:11,'(,ABL:11 * 2,')
	S = 1
ENDIF
;ABL:봉사정신
IF ABL:16
	SIF S
		PRINT  + 
	A += ABL:16 * 4
	PRINTS ABLNAME:16
	PRINTV 'LV,ABL:16,'(,ABL:16 * 4,')
	S = 1
ENDIF
;ABL:정액중독
IF ABL:32
	SIF S
		PRINT  + 
	A += ABL:32 * 1
	PRINTS ABLNAME:32
	PRINTV 'LV,ABL:32,'(,ABL:32 * 1,')
	S = 1
ENDIF

;쾌락각인
IF MARK:1
	SIF S
		PRINT  + 
	A += MARK:1 * 2
	PRINTS MARKNAME:1
	PRINTV 'LV,MARK:1,'(,MARK:1 * 2,')
	S = 1
ENDIF


;PALAM:욕정
IF PALAM:5 < PALAMLV:1
	L = 0
ELSEIF PALAM:5 < PALAMLV:2
	L = 1
ELSEIF PALAM:5 < PALAMLV:3
	L = 2
ELSEIF PALAM:5 < PALAMLV:4
	L = 3
ELSEIF PALAM:5 < PALAMLV:5
	L = 4
ELSE
	L = 5
ENDIF
IF L
	SIF S
		PRINT  + 
	A += L * 3
	PRINTS PALAMNAME:5
	PRINTV 'LV,L,'(,L * 3,')
	S = 1
ENDIF

;수줍음
IF TALENT:35
	PRINT  - 
	A -= 1
	PRINTS TALENTNAME:35
	PRINTV '(,1,')
	S = 1
ENDIF
;쾌감을부정
IF TALENT:71
	PRINT  - 
	A -= 3
	PRINTS TALENTNAME:71
	PRINTV '(,3,')
	S = 1
ENDIF
;남성혐오
IF TALENT:82 && TALENT:PLAYER:122
	PRINT  - 
	A -= 7
	PRINTS TALENTNAME:82
	PRINTV '(,7,')
	S = 1
ENDIF
;애정
IF TALENT:85 && ASSIPLAY == 0
	SIF S
		PRINT  + 
	A += 3
	PRINTS TALENTNAME:85
	PRINTV '(,3,')
	S = 3
ENDIF

;調教者がふたなり
IF TALENT:PLAYER:121
	SIF S
		PRINT  + 
	A += 8
	PRINTS TALENTNAME:121
	PRINTV '(,8,')
	S = 1
ENDIF

;しあわせ草
IF TEQUIP:21
	SIF S
		PRINT  + 
	A += 6
	PRINTS ITEMNAME:26
	PRINTV '(,6,')
	S = 1
ENDIF

Y = 0
;愛液の汚れ
SIF STAIN:PLAYER:2 & 1
	Y += 1
;精液の汚れ
SIF STAIN:PLAYER:2 & 4
	Y += 3
;애널の汚れ
SIF STAIN:PLAYER:2 & 8
	Y += 7
;母乳の汚れ
SIF STAIN:PLAYER:2 & 16
	Y += 1
;尿の汚れ
SIF STAIN:PLAYER:4 & 32
	Y += 3

SIF TALENT:61
	Y /= 3
SIF TALENT:62
	Y *= 2

;兜あわせでは汚れの影響が少なめ
Y /= 3

;더러움 있음
IF Y
	PRINT  - 
	A -= Y
	;악취둔감
	IF TALENT:61
		PRINT 더러움 있음、
		PRINTS TALENTNAME:61
	;악취민감
	ELSEIF TALENT:62
		PRINT 더러움 있음、
		PRINTS TALENTNAME:62
	ELSE
		PRINT 더러움 있음
	ENDIF
	PRINTV '(,Y,')
	S = 1
ENDIF

;合計を表示(20以上で実行)
PRINT  = 
PRINTV A

V = 20
SIF A < V
	PRINT  < 
SIF A == V
	PRINT  = 
SIF A > V
	PRINT  > 
PRINT 실행값
PRINTV V

WAIT

;実行できない
SIF A < V
	RETURN 0

;SAVESTR:22 = 투구맞대기
CALL TRAIN_MESSAGE_B

;-------------------------------------------------
;ソースの計算
;-------------------------------------------------
LOSEBASE:0 += 30
LOSEBASE:1 += 90

SOURCE:12 = 250
SOURCE:13 = 400
SOURCE:14 = 300

;ABL:従順をみる
IF ABL:10 == 0
	SOURCE:11 = 200
ELSEIF ABL:10 == 1
	SOURCE:11 = 120
ELSEIF ABL:10 == 2
	SOURCE:11 = 60
ELSEIF ABL:10 == 3
	SOURCE:11 = 20
ELSEIF ABL:10 == 4
	SOURCE:11 = 0
ELSE
	SOURCE:11 = 0
ENDIF

;ABL:Ｃ感覚をみる
IF ABL:0 == 0
	SOURCE:0 = 20
	SOURCE:4 = 0
	SOURCE:5 = 0
	SOURCE:13 = 20
	TIMES SOURCE:12 , 0.80
ELSEIF ABL:0 == 1
	SOURCE:0 = 80
	SOURCE:4 = 10
	SOURCE:5 = 50
	SOURCE:13 = 20
	TIMES SOURCE:12 , 0.90
ELSEIF ABL:0 == 2
	SOURCE:0 = 350
	SOURCE:4 = 50
	SOURCE:5 = 100
	SOURCE:13 = 20
	TIMES SOURCE:12 , 1.00
ELSEIF ABL:0 == 3
	SOURCE:0 = 750
	SOURCE:4 = 100
	SOURCE:5 = 300
	SOURCE:13 = 20
	TIMES SOURCE:12 , 1.10
ELSEIF ABL:0 == 4
	SOURCE:0 = 1200
	SOURCE:4 = 700
	SOURCE:5 = 600
	SOURCE:13 = 20
	TIMES SOURCE:12 , 1.20
ELSE
	SOURCE:0 = 1750
	SOURCE:4 = 2000
	SOURCE:5 = 1000
	SOURCE:13 = 20
	TIMES SOURCE:12 , 1.30
ENDIF

;ABL:技巧をみる
IF ABL:12 == 0
	TIMES SOURCE:4  , 0.50
	TIMES SOURCE:5  , 0.50
	TIMES SOURCE:13 , 0.50
ELSEIF ABL:12 == 1
	TIMES SOURCE:4  , 0.80
	TIMES SOURCE:5  , 0.80
	TIMES SOURCE:13 , 0.80
ELSEIF ABL:12 == 2
	TIMES SOURCE:4  , 1.00
	TIMES SOURCE:5  , 1.00
	TIMES SOURCE:13 , 1.00
ELSEIF ABL:12 == 3
	TIMES SOURCE:4  , 1.10
	TIMES SOURCE:5  , 1.10
	TIMES SOURCE:13 , 1.10
ELSEIF ABL:12 == 4
	TIMES SOURCE:4  , 1.30
	TIMES SOURCE:5  , 1.30
	TIMES SOURCE:13 , 1.30
ELSE
	TIMES SOURCE:4  , 1.50
	TIMES SOURCE:5  , 1.50
	TIMES SOURCE:13 , 1.50
ENDIF

;調教者のABL:Ｃ感覚をみる
IF ABL:PLAYER:0 == 0
	TIMES SOURCE:4 , 0.80
	TIMES SOURCE:5 , 0.50
ELSEIF ABL:PLAYER:0 == 1
	TIMES SOURCE:4 , 0.90
	TIMES SOURCE:5 , 0.70
ELSEIF ABL:PLAYER:0 == 2
	TIMES SOURCE:4 , 1.00
	TIMES SOURCE:5 , 1.00
ELSEIF ABL:PLAYER:0 == 3
	TIMES SOURCE:4 , 1.10
	TIMES SOURCE:5 , 1.20
ELSEIF ABL:PLAYER:0 == 4
	TIMES SOURCE:4 , 1.20
	TIMES SOURCE:5 , 1.40
ELSE
	TIMES SOURCE:4 , 1.30
	TIMES SOURCE:5 , 1.70
ENDIF

;PALAM:윤활
IF PALAM:3 < PALAMLV:1
	TIMES SOURCE:0 , 0.50
	TIMES SOURCE:5 , 0.60
ELSEIF PALAM:3 < PALAMLV:2
	TIMES SOURCE:0 , 0.75
	TIMES SOURCE:5 , 0.80
ELSEIF PALAM:3 < PALAMLV:3
	TIMES SOURCE:0 , 1.00
	TIMES SOURCE:5 , 1.00
ELSEIF PALAM:3 < PALAMLV:4
	TIMES SOURCE:0 , 1.50
	TIMES SOURCE:5 , 1.20
ELSEIF PALAM:3 < PALAMLV:5
	TIMES SOURCE:0 , 2.00
	TIMES SOURCE:5 , 1.40
ELSE
	TIMES SOURCE:0 , 2.50
	TIMES SOURCE:5 , 1.60
ENDIF

;-------------------------------------------------
;사정ゲージチェック
;-------------------------------------------------
B = 0

;ABL:技巧をみる
IF ABL:12 == 0
	B = 1500
ELSEIF ABL:12 == 1
	B = 2100
ELSEIF ABL:12 == 2
	B = 2900
ELSEIF ABL:12 == 3
	B = 4000
ELSEIF ABL:12 == 4
	B = 5000
ELSE
	B = 6000
ENDIF

;ABL:従順をみる
IF ABL:10 == 0
	TIMES B , 0.80
ELSEIF ABL:10 == 1
	TIMES B , 0.90
ELSEIF ABL:10 == 2
	TIMES B , 1.00
ELSEIF ABL:10 == 3
	TIMES B , 1.10
ELSEIF ABL:10 == 4
	TIMES B , 1.20
ELSE
	TIMES B , 1.30
ENDIF

;PALAM:潤滑をみる
IF PALAM:3 < PALAMLV:1
	TIMES B , 0.50
ELSEIF PALAM:3 < PALAMLV:2
	TIMES B , 0.80
ELSEIF PALAM:3 < PALAMLV:3
	TIMES B , 1.20
ELSEIF PALAM:3 < PALAMLV:4
	TIMES B , 1.50
ELSEIF PALAM:3 < PALAMLV:5
	TIMES B , 1.80
ELSE
	TIMES B , 2.40
ENDIF

;プレイヤーのABL:Ｃ感覚をみる
IF ABL:PLAYER:0 == 0
	TIMES B , 1.00
ELSEIF ABL:PLAYER:0 == 1
	TIMES B , 1.50
ELSEIF ABL:PLAYER:0 == 2
	TIMES B , 2.00
ELSEIF ABL:PLAYER:0 == 3
	TIMES B , 2.50
ELSEIF ABL:PLAYER:0 == 4
	TIMES B , 3.50
ELSE
	TIMES B , 5.00
ENDIF

SIF TALENT:PLAYER:121 || TALENT:PLAYER:122
	BASE:PLAYER:2 += B

;-------------------------------------------------
;사정チェック
;-------------------------------------------------
S = BASE:PLAYER:2
EJAC = MAXBASE:PLAYER:2

IF  S > EJAC * 2
	E = 2
ELSEIF S > EJAC
	E = 1
ELSE
	E = 0
ENDIF

IF E
;사정している
	TIMES SOURCE:4 , 3.00

	;ABL:精液中毒をみる
	IF ABL:32 == 0
		SOURCE:7 = 0
		TIMES SOURCE:5 , 2.00
		TIMES SOURCE:13, 6.00
	ELSEIF ABL:32 == 1
		SOURCE:7 = 500
		TIMES SOURCE:5 , 3.00
		TIMES SOURCE:13, 4.50
	ELSEIF ABL:32 == 2
		SOURCE:7 = 1200
		TIMES SOURCE:5 , 4.00
		TIMES SOURCE:13, 3.50
	ELSEIF ABL:32 == 3
		SOURCE:7 = 3000
		TIMES SOURCE:5 , 6.00
		TIMES SOURCE:13, 3.00
	ELSEIF ABL:32 == 4
		SOURCE:7 = 6000
		TIMES SOURCE:5 , 9.00
		TIMES SOURCE:13, 2.00
	ELSE
		SOURCE:7 = 12000
		TIMES SOURCE:5 , 15.00
		TIMES SOURCE:13, 1.50
	ENDIF
ENDIF

;대량사정
IF E == 2
	TIMES SOURCE:7 , 2.00
	TIMES SOURCE:5 , 1.50

	EXP:PLAYER:3 += 2
	EXP:20 += 9
	PRINTL 대량사정
	PRINTL 정액경험＋９
	;精液汚れ
	STAIN:PLAYER:2 |= 4

	BASE:PLAYER:2 -= EJAC*2
	SIF BASE:PLAYER:2 >= EJAC
		BASE:PLAYER:2 = EJAC-1
		
	;素股で사정させたフラグを流用
	TFLAG:9 = 2
;通常の사정
ELSEIF E == 1
	EXP:PLAYER:3 += 1
	EXP:20 += 3
	PRINTL 사정
	PRINTL 정액경험＋３
	;精液汚れ
	STAIN:PLAYER:2 |= 4

	BASE:PLAYER:2 -= EJAC
	SIF BASE:PLAYER:2 >= EJAC
		BASE:PLAYER:2 = EJAC-1
		
	;素股で사정させたフラグを流用
	TFLAG:9 = 1
ENDIF

;-------------------------------------------------
;汚れ
;-------------------------------------------------
;노예のＰ⇔調教者のＰの汚れが移動
STAIN:2 |= STAIN:PLAYER:2
STAIN:PLAYER:2 |= STAIN:2

IF TALENT:122 == 1
	PRINTS EXPNAME:41
	PRINTL +8
	EXP:41 += 8
ENDIF

SIF ASSIPLAY == 0 && ABL:0 >= 3
		TFLAG:30 += 1

TFLAG:100 = 1

RETURN 1

