;eraIM@Sから導入しました(eramaou)

;-------------------------------------------------
;정상위ＳＰ
;セックス系コマンド：정상위・キスと正常位・胸愛撫と子宮責めの合わせ技
;発動までに時間がかかる上、必要能力も高めだが効果は非常に高い
;愛があると補正が大きい
;-------------------------------------------------
@COM130

SELECTCOM = 130

;処女を奪うかどうかの確認
CALL CONFIRM_LOST_VIRGIN
SIF RESULT == 0
	RETURN 0

;コンドームを使うかの確認
CALL CONFIRM_CONDOM
SIF !RESULT
	RETURN 0
	
PRINTL 정상위ＳＰ
;SAVESTR:22 = 정상위ＳＰ
CALL TRAIN_MESSAGE_B

;V経験を伴うコマンドのフラグ
TFLAG:19 = 1
SIF TALENT:85 && ASSIPLAY == 0 && EXP:0 == 0
	TFLAG:20 = 1

;調教者の사정チェック
CALL COM_EJAC_PLAYER_SEX

;-------------------------------------------------
;ソースの計算
;-------------------------------------------------
LOSEBASE:0 += 70
LOSEBASE:1 += 130

SOURCE:12 = 400

;ABL:奉仕精神をみる
IF ABL:16 == 0
	SOURCE:4 = 50
	SOURCE:5 = 10
ELSEIF ABL:16 == 1
	SOURCE:4 = 150
	SOURCE:5 = 50
ELSEIF ABL:16 == 2
	SOURCE:4 = 200
	SOURCE:5 = 100
ELSEIF ABL:16 == 3
	SOURCE:4 = 250
	SOURCE:5 = 180
ELSEIF ABL:16 == 4
	SOURCE:4 = 300
	SOURCE:5 = 300
ELSE
	SOURCE:4 = 350
	SOURCE:5 = 500
ENDIF

;ABL:技巧をみる
IF ABL:12 == 0
	TIMES SOURCE:4 , 0.50
	TIMES SOURCE:5 , 0.50
ELSEIF ABL:12 == 1
	TIMES SOURCE:4 , 0.80
	TIMES SOURCE:5 , 0.80
ELSEIF ABL:12 == 2
	TIMES SOURCE:4 , 1.00
	TIMES SOURCE:5 , 1.00
ELSEIF ABL:12 == 3
	TIMES SOURCE:4 , 1.50
	TIMES SOURCE:5 , 1.50
ELSEIF ABL:12 == 4
	TIMES SOURCE:4 , 2.50
	TIMES SOURCE:5 , 2.50
ELSE
	TIMES SOURCE:4 , 4.00
	TIMES SOURCE:5 , 4.00
ENDIF

;ABL:Ｃ感覚をみる
IF ABL:0 == 0
	SOURCE:0 = 20
ELSEIF ABL:0 == 1
	SOURCE:0 = 100
ELSEIF ABL:0 == 2
	SOURCE:0 = 500
ELSEIF ABL:0 == 3
	SOURCE:0 = 1200
ELSEIF ABL:0 == 4
	SOURCE:0 = 2000
ELSE
	SOURCE:0 = 2800
ENDIF

;ABL:Ｖ感覚をみる
IF ABL:2 == 0
	SOURCE:1 = 10
	SOURCE:3 = 250
ELSEIF ABL:2 == 1
	SOURCE:1 = 50
	SOURCE:3 = 500
ELSEIF ABL:2 == 2
	SOURCE:1 = 450
	SOURCE:3 = 550
ELSEIF ABL:2 == 3
	SOURCE:1 = 1000
	SOURCE:3 = 800
ELSEIF ABL:2 == 4
	SOURCE:1 = 2800
	SOURCE:3 = 1200
ELSE
	SOURCE:1 = 4000
	SOURCE:3 = 1800
ENDIF

;EXP:Ｖ経験をみる
IF EXP:0 < EXPLV:1
	TIMES SOURCE:1 , 0.20
	SOURCE:6 = 5500
ELSEIF EXP:0 < EXPLV:2
	TIMES SOURCE:1 , 0.60
	SOURCE:6 = 300
ELSEIF EXP:0 < EXPLV:3
	TIMES SOURCE:1 , 1.00
	SOURCE:6 = 50
ELSEIF EXP:0 < EXPLV:4
	TIMES SOURCE:1 , 1.20
	SOURCE:6 = 10
ELSEIF EXP:0 < EXPLV:5
	TIMES SOURCE:1 , 1.30
	SOURCE:6 = 0
ELSE
	TIMES SOURCE:1 , 1.80
	SOURCE:6 = 0
ENDIF

;PALAM:潤滑をみる
IF PALAM:3 < PALAMLV:1
	TIMES SOURCE:1 , 0.10
	SOURCE:6 += 1000
	TIMES SOURCE:6 , 3.00
ELSEIF PALAM:3 < PALAMLV:2
	TIMES SOURCE:1 , 0.40
	SOURCE:6 += 300
	TIMES SOURCE:6 , 1.00
ELSEIF PALAM:3 < PALAMLV:3
	TIMES SOURCE:1 , 1.00
	TIMES SOURCE:6 , 0.50
ELSEIF PALAM:3 < PALAMLV:4
	TIMES SOURCE:1 , 1.40
	TIMES SOURCE:6 , 0.20
ELSEIF PALAM:3 >= PALAMLV:4
	TIMES SOURCE:1 , 1.80
	TIMES SOURCE:6 , 0.10
ENDIF

;調教者がふたなり
IF ASSIPLAY
	SIF TALENT:ASSI:121
		TIMES SOURCE:1 , 2.50
ENDIF

;장신
SIF TALENT:99
	TIMES SOURCE:6 , 0.80
;小柄体形
SIF TALENT:100
	TIMES SOURCE:6 , 2.00
;미숙
SIF TALENT:135
	TIMES SOURCE:6 , 4.00

;정조관념
IF TALENT:30
	IF EXP:0 == 0
		TIMES SOURCE:3 , 0.60
		SOURCE:15 = 10000
	ELSE
		TIMES SOURCE:3 , 0.60
		SOURCE:15 = 1000
	ENDIF
;정조관부실
ELSEIF TALENT:31
	IF EXP:0 == 0
		TIMES SOURCE:3 , 0.60
		SOURCE:15 = 300
	ENDIF
ELSE
	IF EXP:0 == 0
		SOURCE:15 = 3000
	ENDIF
ENDIF

;ABL:Ｂ感覚をみる
IF ABL:1 == 0
	SOURCE:17 = 20
	SOURCE:3 = 50
ELSEIF ABL:1 == 1
	SOURCE:17 = 100
	SOURCE:3 = 100
ELSEIF ABL:1 == 2
	SOURCE:17 = 500
	SOURCE:3 = 160
ELSEIF ABL:1 == 3
	SOURCE:17 = 1200
	SOURCE:3 = 200
ELSEIF ABL:1 == 4
	SOURCE:17 = 2000
	SOURCE:3 = 230
ELSE
	SOURCE:17 = 2800
	SOURCE:3 = 250
ENDIF

;PALAM:欲情をみる
IF PALAM:5 < PALAMLV:1
	TIMES SOURCE:1 , 0.60
	TIMES SOURCE:3 , 0.30
ELSEIF PALAM:5 < PALAMLV:2
	TIMES SOURCE:1 , 0.80
	TIMES SOURCE:3 , 0.60
ELSEIF PALAM:5 < PALAMLV:3
	TIMES SOURCE:1 , 1.00
	TIMES SOURCE:3 , 1.00
ELSEIF PALAM:5 < PALAMLV:4
	TIMES SOURCE:1 , 1.20
	TIMES SOURCE:3 , 1.50
ELSEIF PALAM:5 >= PALAMLV:4
	TIMES SOURCE:1 , 1.50
	TIMES SOURCE:3 , 1.80
ENDIF

;ABL:従順をみる
IF ABL:10 == 0
	TIMES SOURCE:1 , 0.50
	TIMES SOURCE:3 , 0.60
	TIMES SOURCE:15 , 2.00
ELSEIF ABL:10 == 1
	TIMES SOURCE:1 , 0.80
	TIMES SOURCE:3 , 0.80
	TIMES SOURCE:15 , 1.50
ELSEIF ABL:10 == 2
	TIMES SOURCE:1 , 1.00
	TIMES SOURCE:3 , 1.00
	TIMES SOURCE:15 , 1.00
ELSEIF ABL:10 == 3
	TIMES SOURCE:1 , 1.30
	TIMES SOURCE:3 , 1.20
	TIMES SOURCE:15 , 0.80
ELSEIF ABL:10 == 4
	TIMES SOURCE:1 , 1.60
	TIMES SOURCE:3 , 1.40
	TIMES SOURCE:15 , 0.60
ELSE
	TIMES SOURCE:1 , 2.00
	TIMES SOURCE:3 , 1.60
	TIMES SOURCE:15 , 0.30
ENDIF

;TALENT:愛をみる
IF TALENT:85
	TIMES SOURCE:0 , 1.50
	TIMES SOURCE:1 , 1.50
	TIMES SOURCE:3 , 2.00
	TIMES SOURCE:17 , 1.50
ENDIF

;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
;노예の口⇔調教者の口の汚れが移動
STAIN:0 |= STAIN:PLAYER:0
STAIN:PLAYER:0 |= STAIN:0

;노예のＢ⇔調教者の指の汚れが移動
STAIN:5 |= STAIN:PLAYER:1
STAIN:PLAYER:1 |= STAIN:5

;セックス後の処理
CALL COM_AFTER_VAGINA_SEX

RETURN 1
;
