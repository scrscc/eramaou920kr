;eraIM@Sから導入しました(eramaou)

;-------------------------------------------------
;가랑이비비기
;奉仕系コマンド：調教対象が調教者のペニスをクリトリスで刺激
;-------------------------------------------------
@COM33

;調教者が主人で、前回の調教が貝あわせ(조수)かダブル素股だとダブル素股に
IF ASSIPLAY == 0 && (PREVCOM == 70 || (TFLAG:50 && PREVCOM == 63))
	CALL COM_ABLE70
	SIF RESULT == 1
		JUMP COM70
ENDIF

PRINTL 가랑이비비기

;-------------------------------------------------
;実行できるかの判定
;-------------------------------------------------
A = 0
S = 0

;すべての命令に共通の要素を考慮
;(従順が高いと命令に従いやすいなど)
CALL COM_ORDER

;ABL:욕망
IF ABL:11
	SIF S
		PRINT  + 
	A += ABL:11 * 2
	PRINTS ABLNAME:11
	PRINTV 'LV,ABL:11,'(,ABL:11 * 2,')
	S = 1
ENDIF
;ABL:봉사정신
IF ABL:16
	SIF S
		PRINT  + 
	A += ABL:16 * 4
	PRINTS ABLNAME:16
	PRINTV 'LV,ABL:16,'(,ABL:16 * 4,')
	S = 1
ENDIF
;ABL:정액중독
IF ABL:32
	SIF S
		PRINT  + 
	A += ABL:32 * 1
	PRINTS ABLNAME:32
	PRINTV 'LV,ABL:32,'(,ABL:32 * 1,')
	S = 1
ENDIF

;쾌락각인
IF MARK:1
	SIF S
		PRINT  + 
	A += MARK:1 * 2
	PRINTS MARKNAME:1
	PRINTV 'LV,MARK:1,'(,MARK:1 * 2,')
	S = 1
ENDIF


;PALAM:욕정
IF PALAM:5 < PALAMLV:1
	L = 0
ELSEIF PALAM:5 < PALAMLV:2
	L = 1
ELSEIF PALAM:5 < PALAMLV:3
	L = 2
ELSEIF PALAM:5 < PALAMLV:4
	L = 3
ELSEIF PALAM:5 < PALAMLV:5
	L = 4
ELSE
	L = 5
ENDIF
IF L
	SIF S
		PRINT  + 
	A += L * 3
	PRINTS PALAMNAME:5
	PRINTV 'LV,L,'(,L * 3,')
	S = 1
ENDIF

;수줍음
IF TALENT:35
	PRINT  - 
	A -= 1
	PRINTS TALENTNAME:35
	PRINTV '(,1,')
	S = 1
ENDIF
;쾌감을부정
IF TALENT:71
	PRINT  - 
	A -= 3
	PRINTS TALENTNAME:71
	PRINTV '(,3,')
	S = 1
ENDIF
;남성혐오
IF TALENT:82 && TALENT:PLAYER:122
	PRINT  - 
	A -= 7
	PRINTS TALENTNAME:82
	PRINTV '(,7,')
	S = 1
ENDIF
;애정
IF TALENT:85 && ASSIPLAY == 0
	SIF S
		PRINT  + 
	A += 3
	PRINTS TALENTNAME:85
	PRINTV '(,3,')
	S = 3
ENDIF

;調教者がふたなり
IF TALENT:PLAYER:121
	SIF S
		PRINT  + 
	A += 8
	PRINTS TALENTNAME:121
	PRINTV '(,8,')
	S = 1
ENDIF

;しあわせ草
IF TEQUIP:21
	SIF S
		PRINT  + 
	A += 6
	PRINTS ITEMNAME:26
	PRINTV '(,6,')
	S = 1
ENDIF

Y = 0
;愛液の汚れ
SIF STAIN:PLAYER:2 & 1
	Y += 1
;精液の汚れ
SIF STAIN:PLAYER:2 & 4
	Y += 3
;애널の汚れ
SIF STAIN:PLAYER:2 & 8
	Y += 7
;母乳の汚れ
SIF STAIN:PLAYER:2 & 16
	Y += 1
;尿の汚れ
SIF STAIN:PLAYER:4 & 32
	Y += 3

SIF TALENT:61
	Y /= 3
SIF TALENT:62
	Y *= 2

;素股では汚れの影響が少なめ
Y /= 3

;더러움 있음
IF Y
	PRINT  - 
	A -= Y
	;악취둔감
	IF TALENT:61
		PRINT 더러움 있음、
		PRINTS TALENTNAME:61
	;악취민감
	ELSEIF TALENT:62
		PRINT 더러움 있음、
		PRINTS TALENTNAME:62
	ELSE
		PRINT 더러움 있음
	ENDIF
	PRINTV '(,Y,')
	S = 1
ENDIF

;合計を表示(20以上で実行)
PRINT  = 
PRINTV A

V = 20
SIF A < V
	PRINT  < 
SIF A == V
	PRINT  = 
SIF A > V
	PRINT  > 
PRINT 실행값
PRINTV V

WAIT

;実行できない
SIF A < V
	RETURN 0

;SAVESTR:22 = 가랑이비비기
CALL TRAIN_MESSAGE_B

;-------------------------------------------------
;実行決定
;-------------------------------------------------
;-------------------------------------------------
;ソースの計算
;-------------------------------------------------
LOSEBASE:0 += 40
LOSEBASE:1 += 130

SOURCE:13 = 1200
SOURCE:14 = 400

;上のほうで計算した汚れデータ
SOURCE:8 = Y*10 + 60

;ABL:奉仕精神をみる
IF ABL:16 == 0
	SOURCE:4 = 200
	SOURCE:5 = 100
	TIMES SOURCE:8 , 4.00
ELSEIF ABL:16 == 1
	SOURCE:4 = 250
	SOURCE:5 = 180
	TIMES SOURCE:8 , 2.50
ELSEIF ABL:16 == 2
	SOURCE:4 = 300
	SOURCE:5 = 250
	TIMES SOURCE:8 , 1.50
ELSEIF ABL:16 == 3
	SOURCE:4 = 350
	SOURCE:5 = 350
	TIMES SOURCE:8 , 1.00
ELSEIF ABL:16 == 4
	SOURCE:4 = 400
	SOURCE:5 = 500
	TIMES SOURCE:8 , 0.50
ELSE
	SOURCE:4 = 450
	SOURCE:5 = 800
	TIMES SOURCE:8 , 0.10
ENDIF


;ABL:技巧をみる
IF ABL:12 == 0
	TIMES SOURCE:4 , 0.70
	TIMES SOURCE:5 , 0.70
ELSEIF ABL:12 == 1
	TIMES SOURCE:4 , 0.90
	TIMES SOURCE:5 , 0.90
ELSEIF ABL:12 == 2
	TIMES SOURCE:4 , 1.00
	TIMES SOURCE:5 , 1.00
ELSEIF ABL:12 == 3
	TIMES SOURCE:4 , 1.20
	TIMES SOURCE:5 , 1.20
ELSEIF ABL:12 == 4
	TIMES SOURCE:4 , 1.40
	TIMES SOURCE:5 , 1.40
ELSE
	TIMES SOURCE:4 , 1.60
	TIMES SOURCE:5 , 1.60
ENDIF

;ABL:Ｃ感覚をみる
IF ABL:0 == 0
	SOURCE:0 = 0
ELSEIF ABL:0 == 1
	SOURCE:0 = 10
ELSEIF ABL:0 == 2
	SOURCE:0 = 50
ELSEIF ABL:0 == 3
	SOURCE:0 = 200
ELSEIF ABL:0 == 4
	SOURCE:0 = 600
ELSE
	SOURCE:0 = 2000
ENDIF

;PALAM:윤활
IF PALAM:3 < PALAMLV:1
	TIMES SOURCE:0 , 0.30
	TIMES SOURCE:4 , 0.60
ELSEIF PALAM:3 < PALAMLV:2
	TIMES SOURCE:0 , 0.60
	TIMES SOURCE:4 , 0.80
ELSEIF PALAM:3 < PALAMLV:3
	TIMES SOURCE:0 , 1.00
	TIMES SOURCE:4 , 1.00
ELSEIF PALAM:3 < PALAMLV:4
	TIMES SOURCE:0 , 1.50
	TIMES SOURCE:4 , 1.20
ELSEIF PALAM:3 < PALAMLV:5
	TIMES SOURCE:0 , 2.00
	TIMES SOURCE:4 , 1.40
ELSE
	TIMES SOURCE:0 , 2.50
	TIMES SOURCE:4 , 1.60
ENDIF

;-------------------------------------------------
;사정ゲージチェック
;-------------------------------------------------
B = 0

;ABL:技巧をみる
IF ABL:12 == 0
	B = 500
ELSEIF ABL:12 == 1
	B = 1100
ELSEIF ABL:12 == 2
	B = 2000
ELSEIF ABL:12 == 3
	B = 3000
ELSEIF ABL:12 == 4
	B = 3900
ELSE
	B = 4600
ENDIF

;PALAM:윤활
IF PALAM:3 < PALAMLV:1
	B += 0
ELSEIF PALAM:3 < PALAMLV:2
	B += 0
ELSEIF PALAM:3 < PALAMLV:3
	B += 0
ELSEIF PALAM:3 < PALAMLV:4
	B += 300
ELSEIF PALAM:3 < PALAMLV:5
	B += 600
ELSE
	B += 1000
ENDIF

;ABL:従順をみる
IF ABL:10 == 0
	TIMES B , 0.80
ELSEIF ABL:10 == 1
	TIMES B , 0.90
ELSEIF ABL:10 == 2
	TIMES B , 1.00
ELSEIF ABL:10 == 3
	TIMES B , 1.10
ELSEIF ABL:10 == 4
	TIMES B , 1.20
ELSE
	TIMES B , 1.30
ENDIF

;ABL:奉仕技術をみる
IF ABL:13 == 0
	TIMES B , 0.50
ELSEIF ABL:13 == 1
	TIMES B , 0.80
ELSEIF ABL:13 == 2
	TIMES B , 1.20
ELSEIF ABL:13 == 3
	TIMES B , 1.50
ELSEIF ABL:13 == 4
	TIMES B , 1.80
ELSE
	TIMES B , 2.40
ENDIF

;プレイヤーのABL:Ｃ感覚をみる
IF ABL:PLAYER:0 == 0
	TIMES B , 1.00
ELSEIF ABL:PLAYER:0 == 1
	TIMES B , 1.50
ELSEIF ABL:PLAYER:0 == 2
	TIMES B , 2.00
ELSEIF ABL:PLAYER:0 == 3
	TIMES B , 2.50
ELSEIF ABL:PLAYER:0 == 4
	TIMES B , 3.50
ELSE
	TIMES B , 5.00
ENDIF

SIF TALENT:PLAYER:122  || TALENT:PLAYER:121
	BASE:PLAYER:2 += B

;-------------------------------------------------
;사정チェック
;-------------------------------------------------
S = BASE:PLAYER:2
EJAC = MAXBASE:PLAYER:2

IF  S > EJAC * 2
	E = 2
ELSEIF S > EJAC
	E = 1
ELSE
	E = 0
ENDIF

IF E
;사정している
	TIMES SOURCE:4 , 2.00

	;ABL:精液中毒をみる
	IF ABL:32 == 0
		SOURCE:7 = 0
		TIMES SOURCE:5 , 1.50
		TIMES SOURCE:13, 1.40
	ELSEIF ABL:32 == 1
		SOURCE:7 = 200
		TIMES SOURCE:5 , 2.00
		TIMES SOURCE:13, 1.00
	ELSEIF ABL:32 == 2
		SOURCE:7 = 400
		TIMES SOURCE:5 , 2.50
		TIMES SOURCE:13, 0.80
	ELSEIF ABL:32 == 3
		SOURCE:7 = 700
		TIMES SOURCE:5 , 3.00
		TIMES SOURCE:13, 0.50
	ELSEIF ABL:32 == 4
		SOURCE:7 = 1000
		TIMES SOURCE:5 , 4.00
		TIMES SOURCE:13, 0.20
	ELSE
		SOURCE:7 = 1500
		TIMES SOURCE:5 , 5.00
		TIMES SOURCE:13, 0.00
	ENDIF
ENDIF

;대량사정
IF E == 2
	TIMES SOURCE:7 , 1.50
	TIMES SOURCE:5 , 1.20

	EXP:PLAYER:3 += 2
	EXP:20 += 2
	PRINTL 대량사정
	PRINTL 정액경험＋２
	;精液汚れ
	STAIN:PLAYER:2 |= 4

	BASE:PLAYER:2 -= EJAC*2
	SIF BASE:PLAYER:2 >= EJAC
		BASE:PLAYER:2 = EJAC-1

	;素股で사정させたフラグ
	TFLAG:9 = 2
;通常の사정
ELSEIF E == 1
	EXP:PLAYER:3 += 1
	EXP:20 += 1
	PRINTL 사정
	PRINTL 정액경험＋１
	;精液汚れ
	STAIN:PLAYER:2 |= 4

	BASE:PLAYER:2 -= EJAC
	SIF BASE:PLAYER:2 >= EJAC
		BASE:PLAYER:2 = EJAC-1

	;素股で사정させたフラグ
	TFLAG:9 = 1
ENDIF

;-------------------------------------------------
;汚れ
;-------------------------------------------------
;노예のＶ⇔調教者のＰの汚れが移動
STAIN:3 |= STAIN:PLAYER:2
STAIN:PLAYER:2 |= STAIN:3

;-------------------------------------------------
;経験上昇
;-------------------------------------------------
IF TALENT:122 == 0 && TALENT:PLAYER:122 == 0
	PRINTS EXPNAME:40
	PRINTL +7
	EXP:40 += 7
ELSEIF TALENT:122 == 1 && TALENT:PLAYER:122 == 1
	PRINTS EXPNAME:41
	PRINTL +7
	EXP:41 += 7
ENDIF

SIF ASSIPLAY == 0
	TFLAG:30 += 1

;애정경험
E = 1
IF CFLAG:2 >= 1000 && ASSIPLAY == 0
	PRINTFORML %EXPNAME:23%+{E}
	EXP:23 += E
ENDIF
E = 0

;------------------------------------------------
;その他の処理
;------------------------------------------------
TFLAG:100 = 1

;屈服刻印１に相当
TFLAG:200 = 1

;調教者がふたなり
IF TALENT:PLAYER:121
	SOURCE:13 /= 2
ENDIF

RETURN 1
