;eraIM@Sから導入しました(eramaou)

;-------------------------------------------------
;후배위ＳＰ
;セックス系コマンド：서서후배위＋子宮責め
;-------------------------------------------------
@COM134

SELECTCOM = 134

;処女を奪うかどうかの確認
CALL CONFIRM_LOST_VIRGIN
SIF RESULT == 0
	RETURN 0

;コンドームを使うかの確認
CALL CONFIRM_CONDOM
SIF !RESULT
	RETURN 0
	
PRINTL 후배위ＳＰ
;SAVESTR:22 = 후배위ＳＰ
CALL TRAIN_MESSAGE_B

;V経験を伴うコマンドのフラグ(処女膜再生対応)
TFLAG:19 = 1
SIF TALENT:85 && ASSIPLAY == 0 && EXP:0 == 0 && TEQUIP:89 == 0
	TFLAG:20 = 1

;調教者の사정チェック
CALL COM_EJAC_PLAYER_SEX

;-------------------------------------------------
;ソースの計算
;-------------------------------------------------
LOSEBASE:0 += 60
LOSEBASE:1 += 120

SOURCE:12 = 800

;ABL:奉仕精神をみる
IF ABL:16 == 0
	SOURCE:4 = 50
	SOURCE:5 = 10
ELSEIF ABL:16 == 1
	SOURCE:4 = 150
	SOURCE:5 = 50
ELSEIF ABL:16 == 2
	SOURCE:4 = 200
	SOURCE:5 = 100
ELSEIF ABL:16 == 3
	SOURCE:4 = 250
	SOURCE:5 = 180
ELSEIF ABL:16 == 4
	SOURCE:4 = 300
	SOURCE:5 = 300
ELSE
	SOURCE:4 = 350
	SOURCE:5 = 500
ENDIF

;ABL:技巧をみる
IF ABL:12 == 0
	TIMES SOURCE:4 , 0.50
	TIMES SOURCE:5 , 0.50
ELSEIF ABL:12 == 1
	TIMES SOURCE:4 , 0.80
	TIMES SOURCE:5 , 0.80
ELSEIF ABL:12 == 2
	TIMES SOURCE:4 , 1.00
	TIMES SOURCE:5 , 1.00
ELSEIF ABL:12 == 3
	TIMES SOURCE:4 , 1.50
	TIMES SOURCE:5 , 1.50
ELSEIF ABL:12 == 4
	TIMES SOURCE:4 , 2.50
	TIMES SOURCE:5 , 2.50
ELSE
	TIMES SOURCE:4 , 4.00
	TIMES SOURCE:5 , 4.00
ENDIF

;ABL:Ｖ感覚をみる
IF ABL:2 == 0
	SOURCE:1 = 10
	SOURCE:3 = 250
ELSEIF ABL:2 == 1
	SOURCE:1 = 50
	SOURCE:3 = 500
ELSEIF ABL:2 == 2
	SOURCE:1 = 450
	SOURCE:3 = 550
ELSEIF ABL:2 == 3
	SOURCE:1 = 1000
	SOURCE:3 = 800
ELSEIF ABL:2 == 4
	SOURCE:1 = 2800
	SOURCE:3 = 1200
ELSE
	SOURCE:1 = 4000
	SOURCE:3 = 1800
ENDIF

;EXP:Ｖ経験をみる
IF EXP:0 < EXPLV:1
	TIMES SOURCE:1 , 0.20
	SOURCE:6 = 5000

	IF ASSIPLAY && TALENT:PLAYER:122 == 0
		EXP:50 += 1
		PRINTFORML %EXPNAME:50%＋１
	ENDIF
ELSEIF EXP:0 < EXPLV:2
	TIMES SOURCE:1 , 0.60
	SOURCE:6 = 220
ELSEIF EXP:0 < EXPLV:3
	TIMES SOURCE:1 , 1.00
	SOURCE:6 = 30
ELSEIF EXP:0 < EXPLV:4
	TIMES SOURCE:1 , 1.20
	SOURCE:6 = 5
ELSEIF EXP:0 < EXPLV:5
	TIMES SOURCE:1 , 1.30
	SOURCE:6 = 0
ELSE
	TIMES SOURCE:1 , 1.80
	SOURCE:6 = 0
ENDIF

;ABL:Ｂ感覚をみる
IF ABL:1 == 0
	SOURCE:17 = 20
	SOURCE:3 += 50
ELSEIF ABL:1 == 1
	SOURCE:17 = 100
	SOURCE:3 += 100
ELSEIF ABL:1 == 2
	SOURCE:17 = 600
	SOURCE:3 += 200
ELSEIF ABL:1 == 3
	SOURCE:17 = 1400
	SOURCE:3 += 300
ELSEIF ABL:1 == 4
	SOURCE:17 = 2200
	SOURCE:3 += 600
ELSE
	SOURCE:17 = 3200
	SOURCE:3 += 1000
ENDIF

;PALAM:潤滑をみる
IF PALAM:3 < PALAMLV:1
	TIMES SOURCE:1 , 0.10
	SOURCE:6 += 900
	TIMES SOURCE:6 , 3.00
ELSEIF PALAM:3 < PALAMLV:2
	TIMES SOURCE:1 , 0.40
	SOURCE:6 += 250
	TIMES SOURCE:6 , 1.00
ELSEIF PALAM:3 < PALAMLV:3
	TIMES SOURCE:1 , 1.00
	TIMES SOURCE:6 , 0.50
ELSEIF PALAM:3 < PALAMLV:4
	TIMES SOURCE:1 , 1.40
	TIMES SOURCE:6 , 0.20
ELSEIF PALAM:3 >= PALAMLV:4
	TIMES SOURCE:1 , 1.80
	TIMES SOURCE:6 , 0.10
ENDIF

;調教者がふたなり
IF ASSIPLAY
	SIF TALENT:ASSI:121
		TIMES SOURCE:1 , 2.50
ENDIF

;장신
SIF TALENT:99
	TIMES SOURCE:6 , 1.80
;小柄体形
SIF TALENT:100
	TIMES SOURCE:6 , 2.00

;정조관념
IF TALENT:30
	IF EXP:0 == 0
		TIMES SOURCE:3 , 0.60
		SOURCE:15 = 10000
	ELSE
		TIMES SOURCE:3 , 0.60
		SOURCE:15 = 1000
	ENDIF
;정조관부실
ELSEIF TALENT:31
	IF EXP:0 == 0
		TIMES SOURCE:3 , 0.60
		SOURCE:15 = 300
	ENDIF
ELSE
	IF EXP:0 == 0
		SOURCE:15 = 3000
	ENDIF
ENDIF

;PALAM:欲情をみる
IF PALAM:5 < PALAMLV:1
	TIMES SOURCE:1 , 0.60
	TIMES SOURCE:3 , 0.30
ELSEIF PALAM:5 < PALAMLV:2
	TIMES SOURCE:1 , 0.80
	TIMES SOURCE:3 , 0.60
ELSEIF PALAM:5 < PALAMLV:3
	TIMES SOURCE:1 , 1.00
	TIMES SOURCE:3 , 1.00
ELSEIF PALAM:5 < PALAMLV:4
	TIMES SOURCE:1 , 1.20
	TIMES SOURCE:3 , 1.50
ELSEIF PALAM:5 >= PALAMLV:4
	TIMES SOURCE:1 , 1.50
	TIMES SOURCE:3 , 1.80
ENDIF

;ABL:従順をみる
IF ABL:10 == 0
	TIMES SOURCE:1 , 0.50
	TIMES SOURCE:3 , 0.60
	TIMES SOURCE:15 , 2.00
ELSEIF ABL:10 == 1
	TIMES SOURCE:1 , 0.80
	TIMES SOURCE:3 , 0.80
	TIMES SOURCE:15 , 1.50
ELSEIF ABL:10 == 2
	TIMES SOURCE:1 , 1.00
	TIMES SOURCE:3 , 1.00
	TIMES SOURCE:15 , 1.00
ELSEIF ABL:10 == 3
	TIMES SOURCE:1 , 1.30
	TIMES SOURCE:3 , 1.20
	TIMES SOURCE:15 , 0.80
ELSEIF ABL:10 == 4
	TIMES SOURCE:1 , 1.60
	TIMES SOURCE:3 , 1.40
	TIMES SOURCE:15 , 0.60
ELSE
	TIMES SOURCE:1 , 2.00
	TIMES SOURCE:3 , 1.60
	TIMES SOURCE:15 , 0.30
ENDIF

;애정
IF TALENT:85
	TIMES SOURCE:1 , 1.50
	TIMES SOURCE:3 , 2.00
	TIMES SOURCE:17 , 1.50
ENDIF

;後背位スパンキングを挟んでいた場合、マゾっ気補正がさらにかかる
IF TFLAG:59 == 132 || PREVCOM == 132
	;ABL:マゾっ気をみる
	IF ABL:21 == 0
		TIMES SOURCE:1 , 0.60
		TIMES SOURCE:10 , 0.80
		TIMES SOURCE:13 , 0.80
	ELSEIF ABL:21 == 1
		TIMES SOURCE:1 , 0.80
		TIMES SOURCE:10 , 1.00
		TIMES SOURCE:13 , 1.00
	ELSEIF ABL:21 == 2
		TIMES SOURCE:1 , 1.00
		TIMES SOURCE:10 , 1.20
		TIMES SOURCE:13 , 1.50
	ELSEIF ABL:21 == 3
		TIMES SOURCE:1 , 1.10
		TIMES SOURCE:10 , 1.40
		TIMES SOURCE:13 , 3.00
	ELSEIF ABL:21 == 4
		TIMES SOURCE:1 , 1.20
		TIMES SOURCE:10 , 2.00
		TIMES SOURCE:13 , 5.00
	ELSE
		TIMES SOURCE:1 , 1.30
		TIMES SOURCE:10 , 3.00
		TIMES SOURCE:13 , 8.00
	ENDIF
ENDIF

;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
;노예の口⇔調教者の口の汚れが移動
STAIN:0 |= STAIN:PLAYER:0
STAIN:PLAYER:0 |= STAIN:0

;노예のＢ⇔調教者の指の汚れが移動
STAIN:5 |= STAIN:PLAYER:1
STAIN:PLAYER:1 |= STAIN:5

;セックス後の処理
CALL COM_AFTER_VAGINA_SEX

RETURN 1
;
